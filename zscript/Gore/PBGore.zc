class PB_GibNaziLeg: PB_GibPlasmaZombieLeg1
{
    States
    {
		Spawn:
			TNT1 A 0 A_SetRoll(0);
			TNT1 A 0 A_Jump(128, "Spawn1");
			NZL3 B 1 A_SpawnItem("NashGoreBloodSpot",0,0,0,1);
			NZL3 B -1;
			Stop;
		Spawn1:
			NZL3 E 1 A_SpawnItem("NashGoreBloodSpot",0,0,0,1);
			NZL3 E -1;
			Stop;
		Death.plasma:	    
			TNT1 A 0 A_SpawnProjectile ("PB_XDeathBurnedLeg", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 160));   
			Stop;
		Death:    
			TNT1 AAA 0 A_SpawnProjectile ("NashGoreClassicBloodTrail", 0, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 360));
			TNT1 A 0 A_SpawnProjectile ("PB_GibNaziLegPart", 0, 0, random (0, 180), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 180));
			TNT1 A 0 A_SpawnProjectile ("PB_GibNaziBoot", 0, 0, random (0, 180), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 180));
			Stop;
		Death.Kick:
		Death.Melee:
		Death.SuperPunch:
			NZL3 A 1 {
				A_FaceTarget();
				A_SpawnProjectile ("PB_NaziLeg", 5, 0, random (170, 190), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (20, 40));
				}
			Stop;
		
		Pain:
			TNT1 A 0 ThrustThingZ(0,20,0,1);
			NZL3 AAAAAAAA 2 A_SetRoll(roll+30);
			Goto Spawn;
		
		Death.ExplosiveImpact:
			TNT1 A 0 {A_ChangeLinkFlags(False);}
			TNT1 A 0 ThrustThingZ(0,50,0,1);
			NZL3 A 5;
			NZL3 AAA 4 {
				A_SetRoll(roll+30);
				A_CheckFloor("Landz");
			}
			Goto Landz;
		Landz:
			TNT1 A 0 A_SpawnItem("PB_GibNaziLeg");
			Stop;
	}
}

