Class PB_Intestine : PB_TinyGibBase
{
	Default
	{
		Radius 8;
		Height 12;
		Speed 9;
		Mass 5;
		Renderstyle "Translucent";
		Alpha 0.9;
		Gravity 0.5;
		+NOBLOCKMAP;
		+MISSILE;
		+NOTELEPORT;
		+MOVEWITHSECTOR;
		//+CLIENTSIDEONLY;
		+FORCEXYBILLBOARD;
		+RollSprite;
		-EXPLODEONWATER;
		DeathSound "misc/xdeath2";
		Decal "BrutalBloodSplat";
		Scale 0.8;
	}
    States
    {
		Spawn:
			TNT1 A 0;
			GUTS A 1 A_Jump(256, "Spawn1", "Spawn2");
		Spawn1:
			TNT1 A 0 A_SetScale(frandom(0.55,0.85),frandom(0.85,0.85));
			GUTS ABCDEFGHIJ 1 A_SetRoll(roll+10,SPF_INTERPOLATE);
		Spawn1_Flying:
			GUTS J 3 A_SetRoll(roll+10,SPF_INTERPOLATE);
			Loop;
		Spawn2:
			TNT1 A 0 A_SetScale(frandom(0.35,1.15),frandom(0.85,1.25));
			GUTS ABCDEFGHIJ 1 A_SetRoll(roll-10,SPF_INTERPOLATE);
		Spawn2_Flying:
			GUTS J 3 A_SetRoll(roll-10,SPF_INTERPOLATE);
			Loop;
			
	
		Death:
			"####" "#" 0 A_JumpIf(waterlevel > 1, "Water");
			"####" "#" 2 A_CheckFloor("Rest");
			Loop;
		Rest:
			TNT1 A 0 A_SetRoll(0);
			TNT1 A 0 A_Jump(255, "Rest1", "Rest2");
		Rest1:
			GUTS K -1;
			Stop;
		Rest2:
			GUTS L -1;
			Stop;
		Rest3:
			GUTS K 1 {bSPRITEFLIP = true;}
			GUTS K -1;
			Stop;
		Rest4:
			GUTS L 1 {bSPRITEFLIP = true;}
			GUTS L -1;
			Stop;
		
		Water:
			TNT1 A 0 A_Jump(128, "Water2");
		Water1:
			GUTS GH 10 A_SetRoll(roll+20);
			Loop;
		Water2:
			GUTS IJ 10 A_SetRoll(roll-20);
			Loop;
    }
}

