		Crush:
			TNT1 A 0 {
				bYFLIP = false;
				bXFLIP = false;
			}
			TNT1 AAAAAAAA 0 A_SpawnProjectile ("NashGoreClassicBloodTrail", 0, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (60, 140));
