	Void PB_Chaingun_Fire (int firetype)
	{
		switch(firetype)
		{
			default:
			case 0:
			A_SpawnProjectile("OrangeLensFlareAlt", 38, 16, 15, 0);
			A_SpawnProjectile("PB_MonsterMinigunTracer", 38, 0, self.angle + frandom(3,-3), CMF_OFFSETPITCH | CMF_AIMDIRECTION | CMF_ABSOLUTEANGLE | CMF_ABSOLUTEPITCH, self.Pitch - frandom(3,-3));
			break;
			case 1:
			A_SpawnProjectile("OrangeLensFlareAlt", 26, 16, 15, 0);
			A_SpawnProjectile("PB_MonsterMinigunTracer", 26, 0, self.angle + frandom(2,-2), CMF_OFFSETPITCH | CMF_AIMDIRECTION | CMF_ABSOLUTEANGLE | CMF_ABSOLUTEPITCH, self.Pitch - frandom(2,-2));
			break;
			case 2:
			A_FaceTarget(10);
			A_SpawnProjectile("OrangeLensFlareAlt", 38, 16, 15, 0);
			A_SpawnProjectile("PB_MonsterMinigunTracer", 38, 0, self.angle + frandom(4,-4), CMF_OFFSETPITCH | CMF_AIMDIRECTION | CMF_ABSOLUTEANGLE | CMF_ABSOLUTEPITCH, self.Pitch - frandom(4,-4));
			break;
		}
		A_SpawnItem ("PB_EmptyBrass", 0, 30,0);
		A_StartSound("weapons/minigun/chaingunmode/fire", 0, CHANF_OVERLAP);
		PB_DynamicTailMonster("lmg", "br");
	}
	
	void A_CombatDialogueHandler()
	{
		if (target.health < 50)
			A_StartSound("monsters/zombie/player_low", CHAN_AUTO);
		else
			A_StartSound("monsters/zombie/combat", CHAN_AUTO);
	}
	
	override void PostBeginPlay()
	{
		Super.PostBeginPlay(); // call the super function for virtual functions so we don't break shit if GZdoom update.
		HitBoxSpawned = False;
		PickRandomName(RANK_MILITARY);
		minigunspun = false;
		TrackTicker = 0;
	}
	
	override void Tick()
	{
		Super.Tick();
		
		// Update recognition if crouching
		if(IsInState(Self,"Crouching"))
			self.Height = 44;
		else
			self.Height = 56;
		
		//Handle Hitbox system
		//HitboxerHandler();
		
	}
		
