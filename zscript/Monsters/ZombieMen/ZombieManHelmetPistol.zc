		FallingDeath:
			TNT1 A 0 {
				A_Scream();
				A_CustomMissile ("PB_XDeath1", 32, 0, random (0, 360), 2, random (50, 130));
				A_CustomMissile ("PB_XDeath1", 32, 0, random (0, 360), 2, random (50, 130));
				A_CustomMissile ("PB_XDeath1", 32, 0, random (0, 360), 2, random (50, 130));
