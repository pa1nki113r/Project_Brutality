		void A_CombatDialogueHandler()
		{
			if (target == NULL) {
				return;
			}
			
			if (target.health < 50) {
				A_StartSound("monsters/zombie/player_low", CHAN_AUTO);
			}
			else {
				A_StartSound("monsters/zombie/combat", CHAN_AUTO);
			}
		}
		
