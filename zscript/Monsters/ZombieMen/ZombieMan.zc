Class PB_ZombieMan : PB_Monster
{
	Default
	{
		//$Title Rifle Zombieman
		//$Category Project Brutality - Monsters/Former Humans/Zombiemen
		Monster;
		Radius 16;
		Height 56;
		Speed 4;
		FastSpeed 8;
		Mass 100;
		SeeSound "monsters/zombie/see";
		PainSound "monsters/zombie/pain";
		DeathSound "monsters/zombie/death";
		ActiveSound "monsters/zombie/searching";
		Health 80;
		PainChance 200;
		PainChance "Avoid", 150;
		PainChance "Head", 255;
		PainChance "Kick", 255;
		PainChance "ExtremePunches", 255;
		PainChance "Melee", 255;
		PainChance "Taunt", 255;
		PainChance "MonsterKnocked", 255;
		PainChance "ExplosiveImpact", 255;
		PainChance "Trample", 255;
		PainChance "Stun", 255;
		PainChance "Siphon", 255;
		damagefactor "Crush", 5.0;
		damagefactor "Avoid", 1.0;
		damagefactor "GibRemoving", 0.0;
		damagefactor "DontCallTheBaron", 0.0;
		damagefactor "Blood", 0.0; damagefactor "BlueBlood", 0.0; damagefactor "GreenBlood", 0.0;
		damagefactor "Kick", 0.4;
		damagefactor "Shrapnel", 0.4;
		damagefactor "killme", 0.0;
		damagefactor "Taunt", 1.0;
		damagefactor "ExplosiveImpact", 2.2;
		damagefactor "Explosive", 2.2;
		DamageFactor "CauseObjectsToSplash", 0.0;
		damagefactor "SSG", 50.0;
		damagefactor "Shotgun", 1.2;
		damagefactor "Saw", 0.5;
		damagefactor "Cut", 0.75;
		damagefactor "Tear", 0.25;
		damagefactor "TeleportRemover", 0.0;
		damagefactor "Execution", 1000.0;
		DropItem "Rifle";
		DropItem "PB_GrenadeAmmo", 7;
		
		Obituary "%o has been gunned down by a Zombie Soldier.";
		+FLOORCLIP;
		+SLIDESONWALLS;
		+DOHARMSPECIES;
		+HARMFRIENDS;
		-STANDSTILL
		Species "Former_Human";
		
		PB_Monster.PBMonSpeed 4, 3, 1.5, 2;
	}
		
	int movement_direction;
	
	Void PB_Zombie_Fire (Bool Moving = False)
	{
		If (Moving) A_FaceTarget();
		A_SpawnProjectile("PB_RifleBullet", 42, 0, 0, 0);
		If (Moving)
		{
			A_SpawnProjectile("OrangeLensFlareAlt", 42, 16, 15, 0);
			A_SpawnProjectile("MarineMuzzle1", 34, 6);
			ThrustThing(angle*256/360+movement_direction, 1, 0, 0);
		}
		A_SpawnItem ("RifleCaseSpawn", 0, 30,0);
		A_StartSound("weapons/rifle");
	}
	
	void A_CombatDialogueHandler()
	{
		if (target.health < 50)
			A_StartSound("monsters/zombie/player_low", CHAN_AUTO);
		else
			A_StartSound("monsters/zombie/combat", CHAN_AUTO);
	}
	
	override void PostBeginPlay()
	{
		Super.PostBeginPlay(); // call the super function for virtual functions so we don't break shit if GZdoom update.
		HitBoxSpawned = False;
		PickRandomName(RANK_MILITARY);
	}
	
	override void Tick()
	{
		Super.Tick();
		
		// Update recognition if crouching
		if(IsInState(Self,"Crouching"))
			self.Height = 44;
		else
			self.Height = 56;
		
		//Handle Hitbox system
		//HitboxerHandler();
		
	}
		
	States
	{
		Execution:
			TNT1 A 1 A_Die("Execution");
			Stop;
		Death.Execution:
			TNT1 A 0 {
				A_Stopsound();
				A_FaceTarget();
				A_NoBlocking();
			}
			Stop;
		Spawn:
			PSSS A 1;
			TNT1 A 0 {
				A_TakeInventory("EnemyDMRAmmo", 30);
				A_GiveInventory("EnemyDMRAmmo", random(1,12));
				A_GiveInventory("TypeZombieMan", 1);
			}
		Stand:
			TNT1 A 0;
			TNT1 A 0 {
				bNoPain = false;
				A_SetRoll(0);
			}
			PSSS AB 10 A_LookEx();
			Loop;
		See:
			TNT1 A 0 {
				EnemyLastSighted = Level.MapTime;
				if(CountInv("EnemyDMRAmmo") >= 21)
				{
					bFRIGHTENED = true;
				}
				
				bNoPain = false;
				A_SetRoll(0);
				bNODROPOFF = false;
			}
		SeeContinue:
			POSS A 1 A_SmartChase("Missile", "Melee", 0);
			POSS AA 1 A_SmartChase(null, null, CHF_DONTTURN);
			POSS A 1 A_SmartChase("Missile", "Melee", 0);
			POSS AA 1 A_SmartChase(null, null, CHF_DONTTURN);
			POSS B 1 A_SmartChase("Missile", "Melee", 0);
			POSS BB 1 A_SmartChase(null, null, CHF_DONTTURN);
			POSS B 1 A_SmartChase("Missile", "Melee", 0);
			POSS BB 1 A_SmartChase(null, null, CHF_DONTTURN);
			TNT1 A 0 A_StartSound("humans/step");
			POSS C 1 A_SmartChase("Missile", "Melee", 0);
			POSS CC 1 A_SmartChase(null, null, CHF_DONTTURN);
			POSS C 1 A_SmartChase("Missile", "Melee", 0);
			POSS CC 1 A_SmartChase(null, null, CHF_DONTTURN);
			POSS D 1 A_SmartChase("Missile", "Melee", 0);
			POSS DD 1 A_SmartChase(null, null, CHF_DONTTURN);
			POSS D 1 A_SmartChase("Missile", "Melee", 0);
			POSS DD 1 A_SmartChase(null, null, CHF_DONTTURN);
			TNT1 A 0 A_StartSound("humans/step");
			TNT1 A 0 A_JumpIf(CountInv("Wandering") == 71 && !OnPatrol,"LookAround");
			Loop;
		LookAround:
			TNT1 A 0 A_ClearTarget();
			TNT1 A 0 A_TakeInventory("Wandering", 71);
			TNT1 A 0 A_StartSound("monsters/zombie/searching", CHAN_AUTO);
			TNT1 A 0 A_Jump(200, "SeeContinue");
			PSSS ABABCBDB 10 A_Look();
			Goto See;
		FallBack:
			POSS D 4 {
				A_FaceTarget();
				A_Recoil(2);
				A_StartSound("humans/step",3);
				return A_Jump(64,"Missile");
			}
			POSS C 4 {
				A_FaceTarget();
				A_Recoil(2);
				return A_Jump(64,"Missile");
			}
			POSS B 4 {
				A_FaceTarget();
				A_Recoil(2);
				A_StartSound("humans/step",3);
				return A_Jump(64,"Missile");
			}
			POSS A 2 {
				A_FaceTarget();
				A_Recoil(2);
				return A_Jump(64,"Missile");
			}
			POSS D 4 {
				A_FaceTarget();
				A_Recoil(2);
				A_StartSound("humans/step",3);
				return A_Jump(64,"Missile");
			}
			POSS C 4 {
				A_FaceTarget();
				A_Recoil(2);
				return A_Jump(64,"Missile");
			}
			POSS B 4 {
				A_FaceTarget();
				A_Recoil(2);
				A_StartSound("humans/step",3);
				return A_Jump(64,"Missile");
			}
			POSS A 4 {
				A_FaceTarget();
				A_Recoil(2);
			}
			Goto Missile;
		
		////////////////
		//Attack Logic// 
		////////////////
		Melee:
			TNT1 A 0 A_JumpIfInventory("ExecutionToken", 1, "Execution");
			TNT1 A 0 A_Jump(224, 2);
			TNT1 A 0 A_CombatDialogueHandler();
			ZBS1 A 6 A_FaceTarget();
			TNT1 A 0 {
				A_StartSound("skeleton/swing");
				A_CustomMeleeAttack(10, "monsters/nazi/bash", "", "Normal");
			}
			//TNT1 A 0 A_SpawnProjectile("ZombieBashAttack",10,0,0,0);
			ZBS1 B 6 A_FaceTarget();
			ZBS1 A 6;
			Goto See;
		DialogueCheck:
			TNT1 A 0 A_Jump(240, "MissileContinue");
			TNT1 A 0 A_CombatDialogueHandler();
			Goto MissileContinue;
		Missile:
			TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer",1, "DialogueCheck");
			
			Goto AttackHandler;
		MissileContinue:
			POSS E 5 {
				A_FaceTarget();
				return A_CheckLOFRanged("AttackHandler", "SideHop");
				}
			Goto See;
		AttackHandler:
			TNT1 A 0 {
				int chance = (random(1,256));
				
				if((chance > 232) && (CountInv("ZombieManGrenadeCount") < 3)) {return A_Jump(256, "Grenade");}
				if(chance > 200) {return A_Jump(256, "AttackMoving");}
				return A_Jump(256, "Attack1", "Attack1", "Attack2", "Attack2", "Attack3");
			}
			
		//Standard, 3 round burst attack
		Attack1:
			POSS E 4 A_FaceTarget();
			POSS F 2 BRIGHT PB_Zombie_Fire();
			POSS E 4 A_FaceTarget();
			POSS F 2 BRIGHT PB_Zombie_Fire();
			POSS E 4 A_FaceTarget();
			POSS F 2 BRIGHT PB_Zombie_Fire();
			POSS E 4 {
				A_FaceTarget();
				A_GiveInventory("EnemyDMRAmmo", 3);
				return A_JumpIfInventory("EnemyDMRAmmo", 30, "Reload");
				}
			Goto See;
		//Aimed, 3 round burst attack
		Attack2:
			P055 E 5 A_FaceTarget();
			P055 E 2 BRIGHT {
				A_FaceTarget();
				A_SpawnProjectile("PB_RifleBullet", 32, 0, random(-4, 4), 1, random(-3, 3));
				A_SpawnProjectile("OrangeLensFlareAlt", 42, 16, 15, 0);
				A_SpawnProjectile("MarineMuzzle1", 39, 4);
				A_SpawnItem ("RifleCaseSpawn", 0, 36,0);
				A_StartSound("weapons/rifle");
			}
			P055 E 4 A_FaceTarget();
			P055 E 2 BRIGHT {
				A_FaceTarget();
				A_SpawnProjectile("PB_RifleBullet", 32, 0, random(-4, 4), 1, random(-3, 3));
				A_SpawnProjectile("OrangeLensFlareAlt", 42, 16, 15, 0);
				A_SpawnProjectile("MarineMuzzle1", 39, 4);
				A_SpawnItem ("RifleCaseSpawn", 0, 36,0);
				A_StartSound("weapons/rifle");
			}
			P055 E 4 A_FaceTarget();
			P055 E 2 BRIGHT {
				A_FaceTarget();
				A_SpawnProjectile("PB_RifleBullet", 32, 0, random(-4, 4), 1, random(-3, 3));
				A_SpawnProjectile("OrangeLensFlareAlt", 42, 16, 15, 0);
				A_SpawnProjectile("MarineMuzzle1", 39, 4);
				A_SpawnItem ("RifleCaseSpawn", 0, 36,0);
				A_StartSound("weapons/rifle");
			}
			P055 E 4 {
				A_GiveInventory("EnemyDMRAmmo", 3);
				return A_JumpIfInventory("EnemyDMRAmmo", 30, "Reload");
				return A_jump(16, "Missile");
			}
			Goto See;
		// Crouched 3-round burst
		Attack3:
			P055 E 5 A_FaceTarget();
			Crouching:
			P055 F 6 A_FaceTarget();
			P055 F 2 BRIGHT {
				A_SpawnProjectile("PB_RifleBullet", 32, 0, random(-4, 4), 1, random(-3, 3));
				A_SpawnProjectile("OrangeLensFlareAlt", 32, 16, 15, 0);
				A_SpawnProjectile("MarineMuzzle1", 32, 4) ;
				A_SpawnItem ("RifleCaseSpawn", 0, 36,0);
				A_StartSound("weapons/rifle");
			}
			P055 F 4 A_FaceTarget();
			P055 F 2 BRIGHT {
				A_SpawnProjectile("PB_RifleBullet", 32, 0, random(-4, 4), 1, random(-3, 3));
				A_SpawnProjectile("OrangeLensFlareAlt", 32, 16, 15, 0);
				A_SpawnProjectile("MarineMuzzle1", 32, 4) ;
				A_SpawnItem ("RifleCaseSpawn", 0, 36,0);
				A_StartSound("weapons/rifle");
			}
			P055 F 4 A_FaceTarget();
			P055 F 2 BRIGHT {
				A_SpawnProjectile("PB_RifleBullet", 32, 0, random(-4, 4), 1, random(-3, 3));
				A_SpawnProjectile("OrangeLensFlareAlt", 32, 16, 15, 0);
				A_SpawnProjectile("MarineMuzzle1", 32, 4) ;
				A_SpawnItem ("RifleCaseSpawn", 0, 36,0);
				A_StartSound("weapons/rifle");
			}
			P055 F 4 {
				A_FaceTarget();
				A_GiveInventory("EnemyDMRAmmo", 3);
				return A_JumpIfInventory("EnemyDMRAmmo", 30, "Reload");
				return A_jump(24, "Attack3");
			}
			DoneCrouching:
			P055 E 6 A_FaceTarget();
			Goto See;
		AttackMoving:
			POSS E 2 {
				int chance = (random(1,4));
				if(chance == 1) { movement_direction = 192;} //Move Left
				if(chance == 2) { movement_direction = 64;} //Move Right
				if(chance == 3) { movement_direction = 128;} //Move Back
				if(chance == 4) { movement_direction = 0;} //Move Back
			}
			P055 AA 2 {
				A_FaceTarget();
				ThrustThing(angle*256/360+movement_direction, 1, 0, 0);
			}
			P055 B 3 BRIGHT PB_Zombie_Fire (True);
			P055 CC 2 {
				A_FaceTarget();
				ThrustThing(angle*256/360+movement_direction, 1, 0, 0);
			}
			P055 D 3 BRIGHT PB_Zombie_Fire (True);
			P055 AA 2 {
				A_FaceTarget();
				ThrustThing(angle*256/360+movement_direction, 1, 0, 0);
			}

			P055 B 3 BRIGHT PB_Zombie_Fire (True);
			P055 CC 2 {
				A_FaceTarget();
				ThrustThing(angle*256/360+movement_direction, 1, 0, 0);
			}

			P055 D 3;

			POSS E 4 {
				A_Stop();
				A_GiveInventory("EnemyDMRAmmo", 3);
				return A_JumpIfInventory("EnemyDMRAmmo", 30, "Reload");
				return A_jump(12, "Missile");
			}
			Goto See;
			
		Grenade:
			TNT1 A 0 A_JumpIfCloser(500, 1);
			Goto Attack1;
			TNT1 A 0 A_JumpIfCloser(90, "Attack1");
		ThrowGrenade:
			TNT1 A 0 A_StartSound("monsters/zombie/special",CHAN_AUTO);
			P0SG A 6 {
				A_FaceTarget(90,45);
				A_StartSound("OPNGRN", 1);
			}
			P0SG BC 6;
			P0SG D 6 {
				A_FaceTarget(90,45);
				A_DropAdjust("EnemyThrownGrenade","EnemyThrownGrenade",0,18,28);
				A_GiveInventory("ZombieManGrenadeCount", 1);
				A_StartSound("THRGRN", 1);
			}
			P0SG E 6;
			Goto See;
		
		Reload:
			TNT1 A 0 A_Jumpif(CountInv("EnemyDMRAmmo") < 1, "SeeContinue");
			TNT1 A 0 A_StartSound("monsters/zombie/reload",CHAN_AUTO);
			POSS E 4 {
				A_TakeInventory("EnemyDMRAmmo", 30);
				//A_StartSound("Reload", 1);
				bFRIGHTENED = false;
			}
			ID12 C 10;
			ID12 B 5 A_StartSound("weapons/rifle/magout");
			ID12 A 10;
			TNT1 A 0 A_SpawnItemEx("EmptyDMRMag",17,-9.4, 24.4,frandom(-1,1),frandom(-5,-2),frandom(3, 5));
			ID12 B 5;
			ID12 C 5 A_StartSound("weapons/rifle/magin");
			ID12 D 10;
			ID12 C 5 A_StartSound("RIFCL_CK");
			ID12 D 10;
			POSS A 10;
			Goto See;
			
		
		////////////////
		//Pain Logic// 
		////////////////
		Pain.Melee:
		Pain:
			TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Downed");
			POSS G 6 A_Pain();
			TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer",1, "See");
			
			TNT1 A 0 A_Jump(180, "See");
			TNT1 A 0 A_StartSound("monsters/zombie/infight",CHAN_AUTO);
			Goto See;
		
			
		Downed:
			ID11 F 6 A_Pain();
			Goto GetUp;
		Pain.Kick:
		Pain.ExtremePunches:
			TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Pain");
			POS4 U 1 {
					bNODROPOFF = false;
					bFORCEXYBILLBOARD = true;
					A_GiveInventory("IsDown", 1);
					A_Pain();
					A_facetarget();
					ThrustThingZ(0,30,0,1);
			}
			POS4 U 2 A_facetarget();
			POS4 U 1 A_Recoil(15);
			POS4 UVWXY 3;
			Goto FallingAfterImpact;
		
		Pain.ExplosiveImpact:
		Pain.Explosive:
			TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Pain");
			POS4 U 1 {
					bNODROPOFF = false;
					bFORCEXYBILLBOARD = true;
					A_GiveInventory("IsDown", 1);
					A_Pain();
					ThrustThingZ(0,30,0,1);
			}
			POS4 U 2 A_facetarget();
			POS4 U 1 A_Recoil(5);
			POS4 UVWXY 3;
			Goto FallingAfterImpact;
		FallingAfterImpact:
			ID11 F 1 {
				A_GiveInventory ("FallingHeight", 1);
				A_CheckFloor ("GetUp");
			}
			NULL A 0 A_JumpIf (Vel.Z == 0, "GetUp");
			Loop;
		
		
		GetUp:
			TNT1 A 0 A_JumpIfInventory ("FallingHeight", 16, "FallingDie");
			TNT1 A 0 {
				A_TakeInventory("FallingHeight", 100);
				A_Stop();
				A_StartSound("BODYF",6);
			}
			ID11 RRRRRRRRRR 5 A_JumpIf(Vel.Z < 0, "FallingAfterImpact");
			ID11 Q 6 {
				A_FaceTarget();
				A_TakeInventory("IsDown", 1);
				bNODROPOFF = true;
				bFORCEXYBILLBOARD = false;
			}
			ZDE1 D 4;
			P055 F 4;
			POSS E 4;
			TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer",1, "See");
			
			TNT1 A 0 A_StartSound("monsters/zombie/infight",CHAN_AUTO);
			Goto See;
		 
		FallingDie:
			TNT1 A 0 {
				bCOUNTKILL = false;
				bSHOOTABLE = false;
				A_SpawnItem("SplatteredSmall");
			}
			TNT1 A 0 A_JumpIfInventory ("FallingHeight", 32, "Death.Stomp");
			TNT1 A 0 A_JumpIfInventory ("FallingHeight", 25, "FallingSuperDeath");
			TNT1 A 0 A_JumpIfInventory ("FallingHeight", 16, "FallingDeath");
		FallingDeath:
			TNT1 A 0 {
				A_Scream();
				A_SpawnProjectile ("PB_XDeath1", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
				A_SpawnProjectile ("PB_XDeath1", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
				A_SpawnProjectile ("PB_XDeath1", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
				A_SpawnProjectile ("PB_XDeath1", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
				}
			ID11 E 2;
			ID11 F 2 {
				A_StartSound("BODYF",6);
				A_SpawnItem ("GrowingBloodPool");
				A_SpawnItem ("DeadZombieman1");
			}
			Stop;
		FallingSuperDeath:
			TNT1 A 0 A_XScream();
			Goto Death.ExplosiveImpact+1;
				
		Pain.Taunt:
			TNT1 A 0 A_JumpIfInventory("Enraged", 1, "Missile");
			POSS B 1 {
				A_Pain();
				bMISSILEMORE = true;
				bMISSILEEVENMORE = true;
				A_StartSound("grunt/sight");
				A_FaceTarget();
				A_GiveInventory("Enraged", 1);
				}
			POSS B 9;
			Goto Missile;
		Pain.Siphon:
			POSS G 1 {
				A_SpawnItemEx("RedLightning_Small", random (-12, 12), random (-12, 12), random (16, 52), 0, 0);
				A_SpawnItemEx("RedLightning_Small", random (-12, 12), random (-12, 12), random (16, 52), 0, 0);
				A_SpawnItemEx("RedLightning_Small", random (-12, 12), random (-12, 12), random (16, 52), 0, 0);
				A_FaceTarget();
				A_GiveToTarget("HealthBonus",4);
			}
			POSS G 5 {
				A_FaceTarget();
				A_Pain();
				}
			Goto See;
		Possession:
			POSS G 3 {
				A_SetRoll(0);
				A_SetInvulnerable();
				A_Pain();
				}
			"####" "#" 35 ACS_NamedExecuteAlways("Pos - Flicker effect");
			TNT1 A 0 A_UnSetInvulnerable();
			Goto See;

		Pain.Stun:
			POSS G 1 A_Pain();
			POSS GGGGGGGGGG 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
			POSS G 1 A_Pain();
			POSS GGGGGGGGGG 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
			POSS G 1 A_Pain();
			POSS GGGGGGGGGG 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
			POSS G 1 A_Pain();
			POSS GGGGGGGGGG 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
			POSS G 1 A_Pain();
			POSS GGGGGGGGGG 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
			POSS G 1 A_Pain();
			Goto See;
			
		PlayerActionDialogue:
			TNT1 A 0 {
				if (A_JumpIfInTargetInventory("PlayerIsThrowingAGrenade",1, 1)) {
					A_StartSound("monsters/zombie/player_grenade", CHAN_AUTO);
				}
			}
			Goto Avoid;
		
		//New Evade Animation
		DiveLeft:
			POSS A 1 A_FaceTarget();
			POSS A 1 A_SetAngle(angle+90, SPF_INTERPOLATE);
			//POSS A 1 A_SetAngle(angle*256/360+192, SPF_INTERPOLATE);
