

Class PB_ZombieScientistChainsaw : PB_ZombieScientist
{
		Default 
		{
			Health 125;
			MeleeRange 200;
		}
		States
		{
			Spawn:
				5CZA A 1;
				TNT1 A 0 A_GiveInventory("SKChainsawZombie", 1);
			Stand:
				5CZA A 10 A_Look();
				Loop;
			See:
				TNT1 A 0 {
					A_TakeInventory("SKChainsawZombie", 1);
					EnemyLastSighted = Level.MapTime;
				}
			SeeContinue:
				TNT1 A 0 A_StartSound("weapons/chainsaw/idle",7);
				5CZA AABB 2 A_SmartChase();
				TNT1 A 0 A_StartSound("humans/step", 3);
				5CZA CCDD 2 A_SmartChase();
				TNT1 A 0 A_StartSound("humans/step",3);
				TNT1 A 0 A_JumpIfInventory("Wandering",16,"LookAround");
				Loop;
			LookAround:
				TNT1 A 0 A_ClearTarget();
				TNT1 A 0 A_TakeInventory("Wandering", 36);
				TNT1 A 0 A_GiveInventory("SKChainsawZombie", 1);
				TNT1 A 0 A_Jump(200, 2);
				TNT1 A 0 A_StartSound("monsters/zombie/searching", CHAN_AUTO);
				5CZA AAAA 10 A_Look();
				Goto See;
		
		
			////////////////
			//Attack Logic// 
			////////////////
			
			Melee:
				TNT1 A 0 A_JumpIfInventory ("SKLabGuy", 1, "Stand");
				TNT1 A 0 A_Jump(224, 2);
				TNT1 A 0 A_CombatDialogueHandler();
				
				TNT1 A 0 A_JumpIfCloser(80, "MeleeStart");
				TNT1 A 0 A_StartSound("sawswing", 6, CHANF_DEFAULT, 0.5);
			MeleeRushStart:
				TNT1 A 0 A_StartSound("weapons/chainsaw/loop3", 7, CHANF_DEFAULT, 0.7,1);
			MeleeRush:
				TNT1 A 0 A_JumpIfTargetInLos(1);
				Goto MeleeRushGiveUp;
				TNT1 A 0 A_FaceTarget();
				TNT1 A 0 A_CheckLOF(1, CLOFF_NOAIM|CLOFF_SKIPENEMY|CLOFF_SKIPOBJECT , 0, 0, 0, 0, -8);
				Goto Jump;
				5CZC AABB 1 {
					A_FaceTarget();
					A_Recoil(-1);
				}
				TNT1 A 0 A_StartSound("humans/step", 3);
				TNT1 A 0 A_JumpIfCloser(90, "MeleeStart");
				NULL A 0 A_JumpIf (Vel.Z < -8, "InAir");
				5CZC CCDD 1 {
					A_FaceTarget();
					A_Recoil(-1);
				}
				TNT1 A 0 A_StartSound("humans/step", 3);
				TNT1 A 0 A_JumpIfCloser(90, "MeleeStart");
				NULL A 0 A_JumpIf (Vel.Z < -8, "InAir");
				TNT1 A 0 A_JumpIfCloser(500, "MeleeRush");
			MeleeRushGiveUp:
				TNT1 A 0 A_StopSound(7);
				TNT1 A 0 A_StartSound("CSAWSTAT", 7);
				Goto See;
			MeleeStart:
				TNT1 A 0 {
					A_Stop();
					A_StartSound("sawswing", 1, CHANF_DEFAULT, 0.5);
				}
				5CZC GH 3 A_FaceTarget();
			MeleeLoop:
				TNT1 A 0 A_StartSound("weapons/chainsaw/loop", 7, CHANF_DEFAULT, 0.5);
				5CZA EFGHGF 3 { 
					A_FaceTarget();
					A_SpawnProjectile("ZombieChainsawAttack",20,0,0,0);
				}
				TNT1 A 0 A_JumpIfCloser(90, "MeleeLoop");
				TNT1 A 0 A_StopSound(7);
				TNT1 A 0 A_StartSound("CSAWSTAT", 7);
				Goto See;
			
			Jump:
				TNT1 A 0 A_StartSound("jump");
				5CZC E 1 A_FaceTarget();
				5CZC E 2 {
					ThrustThingZ(0,38,0,1);
					A_Recoil (-10);
				}
			InAir:
				5CZC E 2 A_JumpIf (Vel.Z == 0, "Landing");
				Loop;
			Landing:
				TNT1 A 0 A_StartSound("land");
				5CZC F 6;
				5CZA A 4;
				Goto See;
		
		
			////////////////
			//Pain Logic// 
			////////////////
			Pain.Head:
				TNT1 A 0 A_StopSound(7);
				TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Downed");
				TNT1 A 0 A_SpawnProjectile ("NashGoreBloodSpurt", 52, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 160));
				5CZA I 6 {
					A_Pain();
					A_Stop();
				}
				TNT1 AA 0 A_SpawnProjectile ("PB_Teeth", 50, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 160));
				5OSH ABBBA 4;
				TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer",1, "See");
				TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine",1, "See");
				TNT1 A 0 A_Jump(180, "See");
				TNT1 A 0 A_StartSound("monsters/zombie/infight",CHAN_AUTO);
				Goto See;
				
		Pain.Melee:
			TNT1 A 0 A_JumpIfInventory ("SKChainsawZombie", 1, "StealthKill");
			Pain:
				TNT1 A 0 A_StopSound(7);
				TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Downed");
				5CZA I 6 A_Pain();
				TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer",1, "See");
				TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine",1, "See");
				TNT1 A 0 A_Jump(180, "See");
				TNT1 A 0 A_StartSound("monsters/zombie/infight",CHAN_AUTO);
				Goto See;
			StealthKill:
				TNT1 A 0 A_TakeInventory("SKChainsawZombie", 1);
				5CZA I 1 A_FaceTarget();
				TNT1 A 0 A_JumpIfInTargetInventory("FistsSelected", 1, 2);
				5CZA I 1;
				Goto Pain;
				TNT1 A 0 {
					A_GiveToTarget("GoFatality", 1);
					A_GiveToTarget("SKChainsawZombie", 1);
					A_NoBlocking();
					A_Die("Stealthy");
				}
				Stop;
			Death.Stealthy:
				TNT1 A 0;
				Stop;
				
			Downed:
				5S11 C 4;
				TNT1 A 0 A_Pain();
				5S11 AAA 10;
				Goto GetUp;
		
		
		Pain.Kick:
		Pain.ExtremePunches:
			TNT1 A 0 A_StopSound(7);
			TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Pain");
			5CZA G 1 {
					bNODROPOFF = false;
					bFORCEXYBILLBOARD = true;
					A_TakeInventory("SKZombieman", 1);
					A_GiveInventory("IsDown", 1);
					A_Pain();
					A_facetarget();
					ThrustThingZ(0,30,0,1);
			}
			5CZA G 2 A_facetarget();
			5PO4 U 1 A_Recoil(15);
			5PO4 UVWXY 3;
			Goto FallingAfterImpact;
		
		Pain.ExplosiveImpact:
		Pain.Explosive:
			TNT1 A 0 A_StopSound(7);
			TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Pain");
			5PO4 U 1 {
					bNODROPOFF = false;
					bFORCEXYBILLBOARD = true;
					A_TakeInventory("SKZombieman", 1);
					A_GiveInventory("IsDown", 1);
					A_Pain();
					ThrustThingZ(0,40,0,1);
			}
			5PO4 U 2 A_facetarget();
			5PO4 U 1 A_Recoil(5);
			5PO4 UVWXY 3;
			Goto FallingAfterImpact;
		FallingAfterImpact:
			5PO4 Y 1 {
				A_GiveInventory ("FallingHeight", 1);
				A_CheckFloor ("GetUp");
			}
			NULL A 0 A_JumpIf (Vel.Z == 0, "GetUp");
			Loop;
		
		
		GetUp:
			TNT1 A 0 A_SpawnItem("LargeMassWaterImpact");
			TNT1 A 0 A_JumpIfInventory ("FallingHeight", 16, "FallingDie");
			TNT1 A 0 {
				A_TakeInventory("FallingHeight", 100);
				A_Stop();
				A_StartSound("BODYF",6);
			}
			5S11 AAAAAAAAAA 5 A_JumpIf(Vel.Z < 0, "FallingAfterImpact");
			5S11 B 8 {
				A_FaceTarget();
				A_TakeInventory("IsDown", 1);
				bFORCEXYBILLBOARD = false;
			}
			5CZC F 6;
			5CZA A 6;
			TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer",1, "See");
			TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine",1, "See");
			TNT1 A 0 A_StartSound("monsters/zombie/infight",CHAN_AUTO);
			Goto See;
		 
		FallingDie:
			TNT1 A 0 {
				bSHOOTABLE = false;
				bCOUNTKILL = false;
				A_SpawnItem("SplatteredSmall");
			}
			TNT1 A 0 A_JumpIfInventory ("FallingHeight", 32, "Death.Stomp");
			TNT1 A 0 A_JumpIfInventory ("FallingHeight", 25, "FallingSuperDeath");
			TNT1 A 0 A_JumpIfInventory ("FallingHeight", 16, "FallingDeath");
		FallingDeath:
			TNT1 A 0 {
				A_Scream();
				A_SpawnProjectile ("PB_XDeath1", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
				A_SpawnProjectile ("PB_XDeath1", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
				A_SpawnProjectile ("PB_XDeath1", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
				A_SpawnProjectile ("PB_XDeath1", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
				}
			5S11 C 4;
			5SMS L 2 {
				A_StartSound("BODYF",6);
				A_SpawnItem ("GrowingBloodPool");
				A_SpawnProjectile("DroppedChainsaw", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
				A_SpawnItem ("DeadFormerScientistChainsaw");
			}
			Stop;
		FallingSuperDeath:
			TNT1 A 0 A_XScream();
			Goto Death.ExplosiveImpact+1;
				
		Pain.Taunt:
			TNT1 A 0 A_JumpIfInventory("Enraged", 1, "Missile");
			5CZA B 1 {
				A_Pain();
				bMISSILEMORE = true;
				bMISSILEEVENMORE = true;
				A_StartSound("grunt/sight");
				A_FaceTarget();
				A_GiveInventory("Enraged", 1);
				}
			5CZA B 9;
			Goto See;
		Pain.Siphon:
			TNT1 A 0 A_StopSound(7);
			5CZA I 1 {
				A_SpawnItemEx("RedLightning_Small", random (-12, 12), random (-12, 12), random (16, 52), 0, 0);
				A_SpawnItemEx("RedLightning_Small", random (-12, 12), random (-12, 12), random (16, 52), 0, 0);
				A_SpawnItemEx("RedLightning_Small", random (-12, 12), random (-12, 12), random (16, 52), 0, 0);
				A_FaceTarget();
				A_GiveToTarget("HealthBonus",4);
			}
			5CZA I 5 {
				A_FaceTarget();
				A_Pain();
				}
			Goto See;
		Possession:
			TNT1 A 0 A_StopSound(7);
			5CZA I 3 {
				A_SetInvulnerable();
				A_Pain();
				}
			"####" "#" 35 ACS_NamedExecuteAlways("Pos - Flicker effect");
			TNT1 A 0 A_UnSetInvulnerable();
			Goto See;
	
		Pain.Stun:
			TNT1 A 0 A_StopSound(7);
			5CZA I 1 A_Pain();
			5CZA IIIIIIIIII 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
			5CZA I 1 A_Pain();
			5CZA IIIIIIIIII 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
			5CZA I 1 A_Pain();
			5CZA IIIIIIIIII 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
			5CZA I 1 A_Pain();
			5CZA IIIIIIIIII 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
			5CZA I 1 A_Pain();
			5CZA IIIIIIIIII 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
			5CZA I 1 A_Pain();
			Goto See;
			
		PlayerActionDialogue:
			TNT1 A 0 {
				if (A_JumpIfInTargetInventory("PlayerIsThrowingAGrenade",1, 1)) {
					A_StartSound("monsters/zombie/player_grenade", CHAN_AUTO);
				}
			}
			Goto Avoid;
		
		//New Evade Animation
		DiveLeft:
			5CZA A 1 A_FaceTarget();
			5CZA A 1 A_SetAngle(angle*256/360+90, SPF_INTERPOLATE);
			//POSS A 1 A_SetAngle(angle*256/360+192, SPF_INTERPOLATE);
			Goto DivingBegin;
		DiveRight:
			5CZA A 1 A_FaceTarget();
			5CZA A 1 A_SetAngle(angle*256/360-90, SPF_INTERPOLATE);
		//	POSS A 1 A_SetAngle(angle*256/360+64, SPF_INTERPOLATE);
			Goto DivingBegin;
		DiveBack:
			5CZA A 1 A_FaceTarget();
			5CZA A 1 A_SetAngle(angle*256/360-180, SPF_INTERPOLATE);
			//POSS A 1 A_SetAngle(angle*256/360+128, SPF_INTERPOLATE);
			Goto DivingBegin;
		DiveFront:
			5CZA A 1 A_FaceTarget();
			Goto DivingBegin;
		DivingBegin:
			5CZC A 5 A_SetAngle(angle, SPF_INTERPOLATE);
			TNT1 A 0 {
				ThrustThingZ(0,30,0,0);
				ThrustThing(angle, 15, 0, 0);
			}
			5CZC E 8 A_SetAngle(angle, SPF_INTERPOLATE);
		DivingLoop:
			5CZC E 1 A_CheckFloor ("DiveStop");
			5CZC E 1 A_JumpIf (Vel.Z == 0, "DiveStop");
			Loop;
		DiveStop:
			TNT1 A 0 A_StartSound("BODYF", CHAN_AUTO);
			5CZC F 12 A_Stop();
			5CZC A 6;
			Goto See;
		
		//Avoid damage
		Pain.Avoid:
			TNT1 A 0 A_StopSound(7);
			TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer",1, "PlayerActionDialogue");
		Avoid:
			TNT1 A 0 A_Jump(256, "DiveLeft", "DiveRight", "DiveLeft", "DiveRight", "DiveFront");
			Goto DiveFront;
			
	   //If the monster is hit by the "Killme" damage (which is "exploded" by the captured marine), automatically start firing at the target.
		Pain.KillMe:
			TNT1 A 0;
			Goto See;
		//Emergency manuever in case the monster gets killed by an "alerter" actor	
		Death.KillMe:
		Death.Taunt:
		Death.Avoid:
			TNT1 A 0 {
				bTHRUACTORS = true;
				bCOUNTKILL = false;
				A_SpawnItemEx ("PB_ZombieScientistChainsaw",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION |SXF_TRANSFERSPECIAL| SXF_TRANSFERAMBUSHFLAG ,0);
			}
			Stop;
			
		Death:
			TNT1 A 0 A_StopSound(7);
			TNT1 A 0 A_Jump(24, "DeathAccident");
			TNT1 A 0 A_Jump(96, "Death.Arm", "DeathNoGuts");
			TNT1 A 0 A_Jump(256, "DeathMirror", "Death2", "BaseDeath");
		BaseDeath:
			5SMS H 5;
			TNT1 A 0 A_SpawnProjectile("DroppedChainsaw", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
			5SMS I 5 A_Scream();
			5SMS J 5 A_NoBlocking();
			5SMS K 5;
			TNT1 A 0 A_SpawnItem ("GrowingBloodPool");
			TNT1 A 0 A_StartSound("BODYF",6);
			TNT1 A 0 A_SpawnItem ("DeadFormerScientistChainsaw");
			Stop;
		
		Death2:
			5CZA J 5;
			TNT1 A 0 A_SpawnProjectile("DroppedChainsaw", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
			5CZA K 5 A_Scream();
			5CZA L 5 A_NoBlocking();
			5CZA M 5;
			TNT1 A 0 A_SpawnItem ("GrowingBloodPool");
			TNT1 A 0 A_StartSound("BODYF",6);
			TNT1 A 0 A_SpawnItem ("DeadLabguy5CZAN");
			Stop;
	
		DeathMirror:
			5SMS M 5;
			TNT1 A 0 A_SpawnProjectile("DroppedChainsaw", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
			5SMS N 5 A_Scream();
			5SMS O 5 A_NoBlocking();
			5SMS P 5;
			TNT1 A 0 A_SpawnItem ("GrowingBloodPool");
			TNT1 A 0 A_StartSound("BODYF",6);
			TNT1 A 0 A_SpawnItem ("DeadLabguy5SMSQ");
			Stop;
		
		DeathNoGuts:
			TNT1 A 0 A_SpawnProjectile ("MuchBlood", 30, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 160));
			TNT1 AA 0 A_SpawnProjectile ("XDeath1", 30, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 160));
			TNT1 AAA 0 A_SpawnProjectile ("Instestin", 28, 0, random (150, 210), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 40));
			TNT1 A 0 A_SpawnProjectile("DroppedChainsaw", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
			5SMS H 5 A_Scream();
			TNT1 A 0 A_NoBlocking();
			TNT1 A 0 A_SpawnItem ("GrowingBloodPool");
			TNT1 A 0 A_StartSound("BODYF",6);
			TNT1 A 0 A_SpawnItem ("ZombieScientistChainsawDyingGuts");
			Stop;
			
		DeathAccident:
			TNT1 A 0 A_Scream();
			TNT1 A 0 A_NoBlocking();
			//TNT1 A 0 A_SpawnProjectile("DroppedChainsaw", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
			TNT1 A 0 A_SpawnItem ("DyingChainsawScientistAccident");
			Stop;
			

		 Death.Arm:
			TNT1 A 0 {
				A_SpawnProjectile ("XDeathLabGuyArm", 35, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 160));
				A_SpawnProjectile ("PB_SmallGib", 35, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 160));
				A_SpawnProjectile ("PB_SmallGib", 35, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 160));
				A_SpawnProjectile ("PB_MuchBlood2", 35, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 160));
				A_SpawnProjectile("DroppedChainsaw", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
				A_NoBlocking();
				A_Scream();
			}
			5PO5 F 6;
			TNT1 A 0 A_SpawnItem ("DyingFormerScientistChainsawNoArm");
			Stop;
			
			
		
		Death.Saw:
		Death.HKFT:
		Death.cut:
			TNT1 A 0 A_StopSound(7);
			TNT1 A 0 A_Jump(32, "Death.arm");
			TNT1 A 0 A_SpawnProjectile ("PB_MuchBlood", 35, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 160));
			TNT1 A 0 A_SpawnProjectile ("PB_RipZombieScientistChainsaw", 0, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 160));
			TNT1 A 0 A_XScream();
			TNT1 O 0 A_NoBlocking();
			TNT1 A 0 A_SpawnProjectile("DroppedChainsaw", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (50, 130));
			LP07 A 1 {
				A_FaceTarget();
				for (int i = 0; i < 4; i++) {	
					A_SpawnProjectile ("PB_SmallGib", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 40));
					A_SpawnProjectile ("PB_SuperWallRedBlood", 32, 0, random (0, 360), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 40));
				}
				
				A_SpawnProjectile ("PB_Intestine", 32, 0, random (150, 210), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 40));
				A_SpawnProjectile ("PB_Intestine", 32, 0, random (150, 210), CMF_AIMDIRECTION|CMF_ABSOLUTEPITCH|CMF_OFFSETPITCH|CMF_BADPITCH|CMF_SAVEPITCH, random (0, 40));
			}
