				A_SpawnProjectile("PB_PistolBullet", 42, 0, random(-8, 8), 1, random(-3, 3));
				A_SpawnProjectile("OrangeLensFlareAlt", 47, 5, 15, 0);
				A_SpawnProjectile("MarineMuzzle1", 48, 1) ;
