			PSPO E 8 A_FaceTarget();
			PSPO F 3 BRIGHT {
				A_SpawnProjectile("PB_PistolBullet", 42, 1, random(-10, 10), 1, random(-3, 3));
				A_SpawnProjectile("MarineMuzzle1", 47, 1) ;
				A_SpawnProjectile("OrangeLensFlareAlt", 47, 5, 0, 0);
