		Reload:
			TNT1 A 0 A_StartSound("monsters/zombie/reload",CHAN_AUTO);
			PSPO A 4 {
				A_TakeInventory("EnemyPistolAmmo", 15);
				//A_StartSound("PSRLOUT");
