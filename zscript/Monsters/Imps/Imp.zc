		// Attack Handler
		Missile:
			TROO E 6 {
				A_FaceTarget();
				
				int jumpChance = (random(1,256));
				if(jumpChance > 212) {
					if(canJumpOnWallLeft(300)){
						return A_Jump(256, "TurnAndJumpLeft");
					}
					if(canJumpOnWallRight(300)){
						return A_Jump(256, "TurnAndJumpRight");
					}
					if(canJumpOnWallLeft(650)){
						return A_Jump(256, "TurnAndJumpLeft_Long");
					}
					if(canJumpOnWallRight(650)){
						return A_Jump(256, "TurnAndJumpRight_Long");
					}
					if(canJumpOnCeiling(200)) {
						return A_Jump(256, "JumpToCeiling");
					}
				}
				
				
				
				if(GetDistance(true, AAPTR_TARGET) < 250) {
					int pounceChance = (random(1,256));
					if(pounceChance > 192) {
						return A_Jump(256, "PounceAttack");
					}
					
				}
				
				return A_CheckLOF("NormalAttack", CLOFF_MUSTBESOLID|CLOFF_IGNOREGHOST|CLOFF_SKIPOBJECT );
			}
			Goto See;
			
		PounceAttack:
			TROT H 2 {
				A_FaceTarget();
				int direction = (random(0,1));
				if (direction == 1) {
					A_SetAngle(angle+40);
				}
				else {
					A_SetAngle(angle+320);
				}
				crawlingOnFloor = true;
			}
			TROT H 2 A_Recoil(-8);
			TROF DAABBCCBB 2 A_Recoil(-2);
			TNT1 A 0 {
				crawlingOnFloor = False;
				A_Stop();
				A_FaceTarget();
				A_StartSound("weapons/fistwhoosh");
				
			}
			TROF D 1 {
					ThrustThingZ(0,35,0,1);
					A_Recoil (-25);
					A_FaceTarget();
					jumping = true;
			}
			TROF DDDD 2  A_SpawnProjectile("ImpAttack",10,0,0,0);
			
			PounceLoop:
				TROF DDDDDDDDDDDDDDDDDDD 2 A_CheckFloor("PounceEnd");
			PounceEnd:
				TROF B 4 {
					A_Stop();
					jumping = false;
				}
				TROT HD 2 A_FaceTarget();
				Goto See;
				
