		// Check walls on right/left (from Imp's POV) after facing the player
		
		
		bool canJumpOnCeiling(int height) {
			// Is too low to do a jump?
			FLineTraceData RemoteRayCeiling;
			bool ceilingcheck = LineTrace(
			   self.angle,
			   height,
			   self.pitch-90,
			   TRF_THRUACTORS,
			   offsetz: self.height,
			   data: RemoteRayCeiling
			);
			
			if(ceilingcheck && RemoteRayCeiling.HitType == TRACE_HitCeiling ) {
// 				A_PrintBold("Ceiling too low to jump");
				return true;
			}
			return false;
		}
		
		bool tooCloseToFloor() {
			
			FLineTraceData RemoteRayFloor;
			bool floorcheck = LineTrace(
			   self.angle,
			   36,
			   self.pitch+90,
			   TRF_THRUACTORS,
			   offsetz: self.height,
			   data: RemoteRayFloor
			);
			
			if(floorcheck && RemoteRayFloor.HitType == TRACE_HitFloor ) {
				A_PrintBold("Floor too close to wall grab");
				return true;
			}
			return false;
		}
		
		bool touchingWall(){
			// Walls left of Imp's perspective
			FLineTraceData RemoteRay;
			bool wallcheck = LineTrace(
			   self.angle,
			   44,
			   self.pitch,
			   TRF_THRUACTORS,
			   offsetz: self.height,
			   data: RemoteRay
			);
			
			if(wallcheck && RemoteRay.HitType == TRACE_HitWall) {
				return true;
			}
			return false;
		}
		
		bool canJumpOnWallLeft(int range){
			
			// Is too low to do a jump?
			FLineTraceData RemoteRayCeiling;
			bool ceilingcheck = LineTrace(
			   self.angle,
			   128,
			   self.pitch-90,
			   TRF_THRUACTORS,
			   offsetz: self.height,
			   data: RemoteRayCeiling
			);
			
			if(ceilingcheck && RemoteRayCeiling.HitType == TRACE_HitCeiling ) {
// 				A_PrintBold("Ceiling too low to jump");
				return false;
			}
			
		
			// Walls left of Imp's perspective
			FLineTraceData RemoteRay;
			bool wallcheck = LineTrace(
			   self.angle-270,
			   range,
			   self.pitch,
			   TRF_THRUACTORS,
			   offsetz: self.height,
			   data: RemoteRay
			);
			
			if(wallcheck && RemoteRay.HitType == TRACE_HitWall) {
// 				A_PrintBold("Imp Wall Check Left Passed");
				return true;
			}
			return false;
		}
		
		
		// Check walls on right (from Imp's POV) after facing the player
		bool canJumpOnWallRight(int range) {
			// Is too low to do a jump?
			FLineTraceData RemoteRayCeiling;
			bool ceilingcheck = LineTrace(
			   self.angle,
			   128,
			   self.pitch-90,
			   TRF_THRUACTORS,
			   offsetz: self.height,
			   data: RemoteRayCeiling
			);
			
			if(ceilingcheck && RemoteRayCeiling.HitType == TRACE_HitCeiling ) {
// 				A_PrintBold("Ceiling too low to jump");
				return false;
			}
			// Walls right of Imp's perspective
			FLineTraceData RemoteRay;
			bool wallcheck = LineTrace(
			   self.angle-90,
			   300,
			   self.pitch,
			   TRF_THRUACTORS,
			   offsetz: self.height,
			   data: RemoteRay
			);
			
			if(wallcheck && RemoteRay.HitType == TRACE_HitWall) {
// 				A_PrintBold("Imp Wall Check Right Passed");
				return true;
			}
			return false;
		}
		
		
		override void PostBeginPlay() {
			Super.PostBeginPlay(); // call the super function for virtual functions so we don't break shit if GZdoom update.
			HitBoxSpawned = False;
		}
		
		
		override void Tick()
		{
			Super.Tick();
			
			
			
			if( !self.InStateSequence(self.Curstate, self.ResolveState("Spawn")) || 
				!self.InStateSequence(self.Curstate, self.ResolveState("Stand")) ||
				!self.InStateSequence(self.Curstate, self.ResolveState("Pain.Melee"))) {
					canBeStealthKilled = false;
			}
			
			if(!self.InStateSequence(self.Curstate, self.ResolveState("WallLatch_Turret"))) {
				A_SetScale(1.0);
				bMISSILEEVENMORE = true;
				bMISSILEMORE = true;
			}
			else {
				bMISSILEEVENMORE = false;
				bMISSILEMORE = false;
			}
			
			
			if( self.InStateSequence(self.Curstate, self.ResolveState("WallLatchLeft")) || 
				self.InStateSequence(self.Curstate, self.ResolveState("WallLatch_Turret")) ||
				self.InStateSequence(self.Curstate, self.ResolveState("WallLatch_Missile")) ||
				self.InStateSequence(self.Curstate, self.ResolveState("SwitchWallRight")) ||
				self.InStateSequence(self.Curstate, self.ResolveState("SwitchWallLeft")) ||
				self.InStateSequence(self.Curstate, self.ResolveState("WallLatchRight"))) {
				bNoGravity = true;
				bDONTTHRUST = true;
			}
			else {
				bNoGravity = false;
				bDONTTHRUST = false;
			}
			
			// Update recognition if crouching
			if(crawlingOnFloor) {
				self.Height = 28;
			}
			else {
				self.Height = 56;
			}
			
			//Handle Hitbox system
			HitboxerHandler();
			
			//Handle Execution system
			ExecutionHandler();
		}
		
