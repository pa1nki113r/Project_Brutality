ACTOR SmallBrainPieceFast
{
    Radius 2
    Height 2
	Speed 9
	Mass 1
	Scale 0.5
	Decal BrutalBloodSuper
	+MISSILE
    +NOBLOCKMAP
    +NOTELEPORT
	+THRUGHOST
	+CLIENTSIDEONLY
    +DONTSPLASH
    +MOVEWITHSECTOR
	-DONTSPLASH
	+FORCEXYBILLBOARD
	+THRUACTORS
    States
    {
	Spawn:
	TNT1 A 0
	
	TNT1 A 0 A_NoGravity
	TNT1 A 0 A_FaceTarget
	TNT1 A 0 A_Jump(32, "DoNothing")
	XDB5 ABCD 1
	TNT1 A 0 A_Gravity
	Goto See
    See:
		XDB5 ABCD 3
		Loop
	Death:
	TNT1 A 0 ACS_ExecuteAlways(798, 0, 0, 0, 0)//Check Effects
	TNT1 A 0 A_CheckFloor("Rest")
	TNT1 A 2
	Stop
	
	Rest:
	TNT1 A 0
	TNT1 A 0 A_Jump(255, "Rest1", "Rest2")
	Rest1:
        XDT5 B 800
		TNT1 A 0 A_JumpIfInventory("lowgraphicsmode", 1, "DoNothing")
        Loop
		
	Rest2:
		XDT5 O 800
		TNT1 A 0 A_JumpIfInventory("lowgraphicsmode", 1, "DoNothing")
        Loop
	DoNothing:
	TNT1 A 0
	Stop
    }
}



