ACTOR Instestin : PB_TinyGibBase
{
    Radius 8
    Height 12
    Speed 6
	Mass 1
	BounceFactor 0.4
	Renderstyle Translucent
	Alpha 0.9
	+DOOMBOUNCE
    +NOBLOCKMAP
	+MISSILE
    +NOTELEPORT
    +MOVEWITHSECTOR
    +CLIENTSIDEONLY
    +FORCEXYBILLBOARD
	-EXPLODEONWATER
    DeathSound "misc/xdeath2"
	Decal BrutalBloodSplat
	Scale 0.8
    States
    {
    Spawn:
		TNT1 A 0
		TNT1 A 0 A_Jump(256, "Spawn1", "Spawn2", "Spawn3", "Spawn4")
		
	Spawn1:
		GUTS A 4
        Loop
		
	Spawn2:
		GUTS B 4
        Loop
		
	Spawn3:
		GUTS C 4
        Loop
		
	Spawn4:
		GUTS D 4
        Loop	
	
    Death:
	   
        GUTS E 0
		TNT1 A 0 A_JumpIf(waterlevel > 1, "Water")
        
	    Goto Rest
		
		
	Rest:
		TNT1 A 0
		TNT1 A 0 A_SetAngle(random(0,360))
		TNT1 A 0 A_ChangeFlag("FLATSPRITE", 1)
		TNT1 A 0 
		TNT1 A 0 A_Jump(255, "Rest1", "Rest2")
	Rest1:
        GUTS Y -1
		Stop
		
	Rest2:
        GUTS Z -1
		Stop
	DoNothing:
	TNT1 A 0
	Stop
	
	Water:
	TNT1 A 0
	TNT1 A 0 A_Jump(128, "Water2")
	Goto Water1
	
	Water1:
	GUTS GH 10
	//TNT1 A 0 A_JumpIf(waterlevel == 0, "Death")
	Loop
	
	Water2:
	GUTS IJ 10
	//TNT1 A 0 A_JumpIf(waterlevel == 0, "Death")
	Loop
    }
}

