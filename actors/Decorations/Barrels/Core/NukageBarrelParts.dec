Actor BarrelDamageDebug Replaces ExplosiveBarrel {
	+LOOKALLAROUND
	States {
	Spawn:
		TNT1 A 1
		TNT1 A 0 A_JumpIf(GetCvar("pb_LuckyBarrels") == 0, "SPNB")
		TNT1 A 0 A_Jump(24, "SPFB")
		TNT1 A 0 A_Jump(12, "SPIB")
		Goto SPNB
	SPNB:
		TNT1 A 0 A_SpawnItemEx("Nbsp12DONTDELETE",0,0,0,0,0,0,0,SXF_TRANSFERSPECIAL | SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERPOINTERS | 288,0,tid)
		Stop
	SPFB:
		TNT1 A 0 A_SpawnItemEx("Fbsp12DONTDELETE",0,0,0,0,0,0,0,SXF_TRANSFERSPECIAL | SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERPOINTERS | 288,0,tid)
		Stop
	SPIB:
		TNT1 A 0 A_SpawnItemEx("Ibsp12DONTDELETE",0,0,0,0,0,0,0,SXF_TRANSFERSPECIAL | SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERPOINTERS | 288,0,tid)
		Stop	}}
