actor GrowingNukagePool {
	game Doom
	radius 1
	height 1
	mass 1
	Health 600
	Alpha 0.99
	Scale 0.5
	Decal GreenBloodSplat
	Renderstyle Normal
	+NOTELEPORT
	+CLIENTSIDEONLY
	+FORCEXYBILLBOARD
	+MOVEWITHSECTOR
	+FLOORCLIP
	+MISSILE
	-DONTSPLASH
	translation "168:191=112:127", "16:47=123:127"
	 states {
		 Spawn:		
			TNT1 A 0 A_JumpIf(waterlevel > 1, "Splash")
			BPDL A 1
			BPDL A 2
			BPDL BCD 2
			BPDL EFGHIJKLMNOP 2
			BPDL QRSTUVWXYZ 2
		 Live:
			BPDL Z 900//duration
			loop
		Splash:
			BLOD A 0
			stop
		SpawnLiquid:
			TNT1 A 0 
			TNT1 A 0 A_SpawnItemEx ("WaterBloodSpotGreen",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0)
			Stop	}}
			
