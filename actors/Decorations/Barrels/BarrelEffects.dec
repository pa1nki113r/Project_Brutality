ACTOR BarrelShrapnel1
{
    Radius 2
    Height 2
    Speed 8
	Mass 1
    +NOBLOCKMAP
	+MISSILE
    +NOTELEPORT
    +MOVEWITHSECTOR
    +CLIENTSIDEONLY
	+THRUACTORS
	+FLOORCLIP
	+DOOMBOUNCE
	BounceFactor 0.5
	Gravity 0.9
	Mass 1
	Scale 0.5
    States
    {
    Spawn:
	    TNT1 A 0 A_JumpIf(waterlevel > 1, "Underwater")
        BFMT A 16{
				A_SetRoll(roll+frandom(-80,-120));
				}
        Goto Spawn2
	Spawn2:
	    TNT1 A 0 A_JumpIf(waterlevel > 1, "Underwater")
	    BFMT A 16{
				A_SetRoll(roll+frandom(-80,-120));
				}
		Loop
		
    Death:
        TNT1 A 0
		BFMT A 300
		BFMT AAAAAAAAA 2 A_FadeOut(0.1)
        Stop
     Underwater:
	 Splash:
	    Goto Death
    }
}

