		Stop
		
	 Active:
        "####" G 1 A_FaceTarget
		"####" A 0 A_JumpIfInTargetInventory("HasBarrel",1, "See")
		"####" A 0 A_JumpIfInTargetInventory("HasIceBarrel",1, "See")
