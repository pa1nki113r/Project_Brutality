ACTOR IceCrystalGround
{
	+SOLID
	+FLOORHUGGER
	+NOBLOOD
	+FORCEXYBILLBOARD 
	Gravity 500
	Mass 10000000000
	Radius 5
	Health 10
	Height 5
	Monster
	Speed 0
	Scale 0.8
	PainChance 256
	BloodType "XIceChunk1"
	DamageFactor "Ice", 0.0
	DamageFactor "Freeze", 0.0
	DamageFactor "IceBeam", 2.5
	DamageFactor "bloodsplasher", 0.0    
	DamageFactor "tinybloodsplasher", 0.0    
	DamageFactor "superbloodsplasher", 0.0    
	DamageFactor "smallbloodsplasher", 0.0
	DamageFactor "Blood", 0.0 
	DamageFactor "BlueBlood", 0.0 
	DamageFactor "GreenBlood", 0.0
	DamageFactor "BlueBloodSplasher", 0.0
    DamageFactor "BlueSuperBloodSplasher", 0.0
	DeathSound "IceBreakSmall"
	States
	{
	Spawn:
		TNT1 A 0 A_Jump(116, "Flipped")
		ICRL A 1
		ICRL A 6 
		ICRL A 650 
		Goto Death
	Flipped:
		TNT1 A 0 A_SetScale(scalex*(-1),scaley)
		ICRL A 1
		ICRL A 6 
		ICRL A 650 
		Goto Death
	Death:
		TNT1 A 1 A_PlaySound ("IceBreakSmall")
		TNT1 A 0 A_CustomMissile ("IceShard1", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("IceShard2", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("IceShard3", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("IceShard4", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("IceShard5", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("IceShard6", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("IceShard7", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("IceShard8", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("IceShard9", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("IceShard10", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("IceShard11", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("IceShard12", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 10
		Stop
	Death.IceBeam:
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		ICRL A 1
		TNT1 A 0 A_SpawnItem("IceCrystal2")
		ICRL AAAAAAAAAA 1 A_FadeOut(0.1)
		Stop
	Death.Fire:
	Death.Flames:
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		ICRL A 1 A_PlaySound ("IceMelt")
		ICRL AAAAAAAAAA 1 A_FadeOut(0.1)
		TNT1 AAA 0 A_CustomMissile ("WaterParticleX", 2, random(-5,5), random (0, 360), 2, random (50, 140))
		Stop
	}
}

