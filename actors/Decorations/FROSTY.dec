		ICRL N 1
		ICRL N 6 
		ICRL N 500 
		Goto Death
	Death:
		TNT1 A 1 A_PlaySound ("IceBreakLarge")
		TNT1 AAAA 0 A_CustomMissile ("IceShard1", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("IceShard2", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("IceShard3", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("IceShard4", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("IceShard5", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("IceShard6", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("IceShard7", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("IceShard8", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("IceShard9", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("IceShard10", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("IceShard11", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("IceShard12", 5, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 10
		Stop
	Death.Fire:
	Death.Flames:
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		ICRL N 1 A_PlaySound ("IceMelt")
		ICRL NNNNNNNNNN 1 A_FadeOut(0.1)
		TNT1 AAA 0 A_CustomMissile ("WaterParticleX", 2, random(-5,5), random (0, 360), 2, random (50, 140))
		Stop
	}
}

