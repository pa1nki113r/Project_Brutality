ACTOR 2Shells : NewShell
{
	Scale .6
	Inventory.PickupMessage "Picked up a pair of Shotgun Shells."
	Inventory.PickupSound "misc/shell_PickUp"
	Inventory.Amount 2
	+DONTGIB
	+FLOORCLIP
	States
	{
	Spawn:
		SH3L B -1
		Stop
	}
}

