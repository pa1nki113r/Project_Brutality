ACTOR LiveShellRed_K : CustomInventory //NewShell
{
	Scale 0.15
	Inventory.PickupMessage "Picked up a Single Shotgun Shell."
	Inventory.PickupSound "misc/shell_PickUp"
	//Inventory.Amount 1
	+DONTGIB
	+FLOORCLIP
	States
	{
	Spawn:
		CAS2 K -1
		Stop
	Pickup:
		TNT1 A 0 A_GiveInventory ("PB_Shell", 1)
		stop
	}
}

