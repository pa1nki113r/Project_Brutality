ACTOR NanoStim : Health
{
	+DONTGIB
	+FLOORCLIP
	Scale 0.03
	Inventory.Amount 5
	Inventory.MaxAmount 999
	Inventory.PickupMessage "UAC Nanobot Injector!"
	Inventory.PickupSound "Syringe/Pickup"
	States
	{
	Spawn:
		NANO ABCD 6
		Loop
	}
}