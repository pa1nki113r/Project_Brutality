ACTOR BarrelThrower : PB_Weapon
{
	Weapon.AmmoUse1 0
	Weapon.AmmoGive1 0
	Weapon.AmmoType1 "None"
	Weapon.AmmoType2 "None"
	+WEAPON.MELEEWEAPON
	+WEAPON.NOALERT
	Weapon.Sisterweapon "none"
	States
	{
		Ready:
		Ready3:
		TNT1 A 0 A_JumpIfInventory("HasBarrel", 1, "ReadyBarrel")
		TNT1 A 0 A_JumpIfInventory("HasIceBarrel", 1, "ReadyIceBarrel")
		TNT1 A 0 A_JumpIfInventory("HasBurningBarrel", 1, "ReadyBurningBarrel")
	ReadyBarrel:
		BANE ABCDEFG 1
		TNT1 A 0 A_PlaySound("barrel/pain")
		Goto IdleBarrel	
	ReadyBurningBarrel:
		BAFE ABCDEFG 1
		TNT1 A 0 A_PlaySound("barrel/pain")
		Goto IdleBurningBarrel	
	ReadyIceBarrel:
		BAIE ABCDEFG 1
		TNT1 A 0 A_PlaySound("barrel/pain")
		Goto IdleIceBarrel
		
	IdleBarrel:
        THRG LKJIHIJKL 2 {
			A_WeaponReady;
			if(CountInv("Kicking") >= 1) {return state("DoKick");}
			return state("");
			}
		loop
		
	IdleBurningBarrel:
        THRG DEFGFED 2 {
			A_WeaponReady;
			if(CountInv("Kicking") >= 1) {return state("DoKick");}
			return state("");
			}
		loop
		
	IdleIceBarrel:
        THRG M 1 {
			A_WeaponReady;
			if(CountInv("Kicking") >= 1) {return state("DoKick");}
			return state("");
			}
		loop
		
	Select:
	    TNT1 A 0 A_TakeInventory("MeatAmmo", 100)
		TNT1 A 0 A_Overlay(-10, "FirstPersonLegsStand")
		
		TNT1 AAAAAAAAAAAAAAAAAA 0 A_Raise
		TNT1 AAAAAAAA 1 A_Raise
		Wait
		
    Fire:
		TNT1 A 0 A_JumpIfInventory("PowerStrength",1,"PreparingToThrow")
		Goto AltFire
		
	PreparingToThrow:
		TNT1 A 0 A_JumpIfInventory("HasBarrel", 1, "FireBarrel")
		TNT1 A 0 A_JumpIfInventory("HasIceBarrel", 1, "FireIceBarrel")
		TNT1 A 0 A_JumpIfInventory("HasBurningBarrel", 1, "FireBBarrel")
	FireBarrel:
		BANE HIJKLMN 1
		TNT1 A 0 A_PlaySound("weapons/fistwhoosh")
		BANE ONLJH 1
		TNT1 A 0 A_JumpIfInventory("GoFatality", 1, "Steady")
		TNT1 A 0 A_FireCustomMissile("ThrowedBarrel", 0, 1, 0, 0)
		TNT1 A 0 A_TakeInventory("HasBarrel",1)
		TNT1 A 0 A_TakeInventory("GrabbedBarrel",1)
        THRF ABCDEF 2
		TNT1 A 0 A_TakeInventory("BarrelThrower",1)
	stop
	FireBBarrel:
		BAFE HIJKLMN 1
		TNT1 A 0 A_PlaySound("weapons/fistwhoosh")
		BAFE ONLJH 1
		TNT1 A 0 A_JumpIfInventory("GoFatality", 1, "Steady")
		TNT1 A 0 A_FireCustomMissile("ThrowedBurningBarrel", 0, 1, 0, 0)
		TNT1 A 0 A_TakeInventory("HasBurningBarrel",1)
		TNT1 A 0 A_TakeInventory("GrabbedBarrel",1)
        THRF ABCDEF 2
		TNT1 A 0 A_TakeInventory("BarrelThrower",1)
		stop
		
	FireIceBarrel:
		BAIE HIJKLMN 1
		TNT1 A 0 A_PlaySound("weapons/fistwhoosh")
		BAIE ONLJH 1
		TNT1 A 0 A_JumpIfInventory("GoFatality", 1, "Steady")
		TNT1 A 0 A_FireCustomMissile("ThrowedIceBarrel", 0, 1, 0, 0)
		TNT1 A 0 A_TakeInventory("HasIceBarrel",1)
		TNT1 A 0 A_TakeInventory("GrabbedBarrel",1)
        THRF ABCDEF 2
		TNT1 A 0 A_TakeInventory("BarrelThrower",1)
		stop
	
	Deselect:
	Altfire:
		TNT1 A 0 A_JumpIfInventory("HasBarrel", 1, "AltFireBarrel")
		TNT1 A 0 A_JumpIfInventory("HasIceBarrel", 1, "AltFireIceBarrel")
		TNT1 A 0 A_JumpIfInventory("HasBurningBarrel", 1, "AltFireBBarrel")
	
