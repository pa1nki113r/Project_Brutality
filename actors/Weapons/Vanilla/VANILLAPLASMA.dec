// Let Fly --------------------------------------------------------------------------

	TNT1 A 0 A_StopSound(6)
	415F A 1 A_PlaySound("PLSULT")
		TNT1 A 0 A_ZoomFactor(0.9)
	RIFF A 0 A_FireCustomMissile("GunFireSmoke", 0, 0, 0, 0, 0, 0)
