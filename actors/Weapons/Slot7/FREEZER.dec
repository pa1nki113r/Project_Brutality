			FR30 JKLMNOPQ 1 {
				if (CountInv("FreezerOverCooling") >= 100 ) { A_SetWeaponSprite("FR39");}
			}
		FinalDeselect:
			FR93 NOPQ 0
			FR03 NOPQ 1 {
				if (CountInv("FreezerOverCooling") >= 100 ) { A_SetWeaponSprite("FR93");}
			}
			TNT1 A 0 A_Takeinventory("FreezerOverCooling",420)
