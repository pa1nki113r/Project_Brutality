			}
			Goto Ready3
		
		UnequipPistol:
			TNT1 A 0 {
				A_WeaponOffset(0,32);
				A_TakeInventory("FreezerPistolToken", 1);
				A_PlaySoundEx("Ironsights", "Auto");
			}
			FR30 JKLMNOPQ 1 {
				if (CountInv("FreezerOverCooling") >= 100 ) { A_SetWeaponSprite("FR39");}
			}
			Goto Ready3
		
