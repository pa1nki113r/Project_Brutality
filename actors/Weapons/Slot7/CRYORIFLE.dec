Actor CryoRiflePistolToken : Inventory {inventory.maxamount 1}
ACTOR FireModeCryoRifleMissile : Inventory { Inventory.MaxAmount 1 }
ACTOR FireModeCryoRifleBeam : Inventory { Inventory.MaxAmount 1 }
ACTOR FireModeCryoRifleSpear : Inventory { Inventory.MaxAmount 1 }
ACTOR FireModeCryoRifleFlak : Inventory { Inventory.MaxAmount 1 }
ACTOR FireModeCryoRifleMissile_WW : Inventory { Inventory.MaxAmount 1 }
ACTOR FireModeCryoRifleBeam_WW : Inventory { Inventory.MaxAmount 1 }
ACTOR FireModeCryoRifleSpear_WW : Inventory { Inventory.MaxAmount 1 }
ACTOR FireModeCryoRifleFlak_WW : Inventory { Inventory.MaxAmount 1 }
ACTOR RespectCryoRifle : Inventory { Inventory.MaxAmount 1 }
ACTOR HasUnloadedCryoRifle : Inventory { Inventory.MaxAmount 1 }
Actor CryoRifleOverCooling: Inventory { Inventory.maxamount 420 }
Actor CryoRifleAmmo : Ammo {
inventory.maxamount 60
+INVENTORY.IGNORESKILL
Inventory.Icon "FRPKA0"
}

// The LNDD - Mark III, colloquially known as the Cryo Rifle is an experimental weapon 
//developed by the TeiTenga Armaments corporation. It was developed to combat the more
//flammable denizens of hell. Powered by plasma energy, it injects fuel into a powerful
//vaccum chamber, where the superheated plasma quickly burns both its own fuel, and
//all oxygen in the chamber. This drops the chamber to temperatures below that of the
//vaccum of space. The projectiles fired by the weapon are actually comprised of 
//moisture from the air, as suddenly releasing the chamber's pressure flash freezes all
//liquid in a nearby area. Anything outside of this radius will be coated with 
//supercooled particulate at enough volume to generate ice crystals on flesh. The weapon
//comes equipped with an adjustable lens on the barrel to release the supercooled plasma 
//gas in a number of different orientations, allowing the user to create projectiles
//from ice when focussed enough, or the spray a beam of freezing gasses when unfocussed.
Actor PB_CryoRifle: PB_Weapon {
	//$Title TeiTenga Arms Cryo Rifle
	//$Category Weapons
	//$Sprite FRPKA0
	SpawnID 9700
	Weapon.BobRangeX 0.3
	Weapon.BobRangeY 0.5
	Weapon.BobStyle InverseSmooth
	Weapon.BobSpeed 2.4
	Weapon.SelectionOrder 100
	Weapon.AmmoUse1 0
	Weapon.AmmoGive1 60
	Weapon.AmmoUse2 0
	Weapon.AmmoGive2 0
    Inventory.PickupSound "weapons/CryoRifle/respect1"
	Weapon.AmmoType1 "Cell"
	Weapon.AmmoType2 "CryoRifleAmmo"
   	+WEAPON.NOAUTOAIM
	+WEAPON.NOAUTOFIRE
	+WEAPON.NOALERT
	DamageType "Ice"
	Inventory.PickupMessage "You got the TeiTenga Arms Cryo Rifle! (Slot 8)"
	Obituary "%o was iced by %k"
	Tag "TeiTenga Arms Cryo Rifle"
	Scale 0.48
	PB_WeaponBase.respectItem "RespectCryoRifle"
		States {
  	
			Spawn:
				FRPK A 1
				Loop
		
			Steady: 
				TNT1 A 1
				Goto Ready
				
			Ready:
				TNT1 A 0 PB_RespectIfNeeded
			WeaponRespect:
				TNT1 A 0 {
					A_SetCrosshair(5);
					A_GiveInventory("RespectCryoRifle");
					A_GiveInventory("FireModeCryoRifleMissile");
				}
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/up", "Auto")
				FR00 ABCDEFGHIJKLMNO 1 A_DoPBWeaponAction
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect2", "Auto")
				FR00 PQRSTUVWXYZ 1 A_DoPBWeaponAction
				FR01 ABCD 1 A_DoPBWeaponAction
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect3", "Auto")
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect1", "Auto")
				FR01 EFGHIJKLMNOPQRSTUVWXYZ 1 {
					A_FireCustomMissile("GunFireSmoke", 0, 0, -2, -5, 0, 0);
					return A_DoPBWeaponAction;
				}
				FR02 ABCDEFGH 1 {
					A_FireCustomMissile("GunFireSmoke", 0, 0, -2, -5, 0, 0);
					return A_DoPBWeaponAction;
				}
				FR02 IJKL 1 A_DoPBWeaponAction
				TNT1 A 0 A_PlaySound("weapons/CryoRifle/idle", 1, 1.0, 1)
				FR02 MNOPQRSTUVWXYZ 1 A_DoPBWeaponAction
				FR60 ABCDEFGHIJKLMNOPQ 1 A_DoPBWeaponAction
				Goto Ready3
				
			SelectAnimation:
				TNT1 A 0 A_JumpIfInventory("GoFatality", 1, "Steady")
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect1", "Auto")
				FR03 ABCD 1
			Ready3:
				TNT1 A 0 {
					A_TakeInventory("PB_LockScreenTilt",1);
					PB_HandleCrosshair(79);
					A_PlaySound("weapons/CryoRifle/idle", 1, 1.0, 1);
					A_PlaySound("PLSIDLE", 6, 0.7, 1);
					}
				TNT1 A 0 A_JumpIfInventory("CryoRiflePistolToken", 1, "Ready2")
				TNT1 A 0 A_JumpIfInventory("HasUnloadedCryoRifle", 1, "GunEmpty")
			ReadyToFire1:
				FR03 EFGHIJKLMMLKJIHGFE 1 {
					A_Overlay(7, "BigTubeGlow");
					A_Overlay(8, "SmallTubeGlow");
					A_Overlay(9, "MuzzleGlow");
					A_TakeInventory("CryoRifleOverCooling",1);
					A_FireCustomMissile("TinyGunSmoker", 0, 0, 0, -3, 0, 0);
					return A_DoPBWeaponAction(WRF_ALLOWRELOAD);
				}
				FR03 EFGHIJKLMMLKJIHGFE 1 {
					A_Overlay(7, "BigTubeGlow");
					A_Overlay(8, "SmallTubeGlow");
					A_Overlay(9, "MuzzleGlow");
					A_TakeInventory("CryoRifleOverCooling",1);
					A_FireCustomMissile("TinyGunSmoker", 0, 0, 0, -3, 0, 0);
					return A_DoPBWeaponAction(WRF_ALLOWRELOAD);
				}
				Loop
			GunEmpty:
				TNT1 A 0 A_StopSound(1)
				TNT1 A 0 A_StopSound(6)
				FR03 EE 1 A_DoPBWeaponAction(WRF_ALLOWRELOAD)
				Goto GunEmpty+3
			
			//Pistol Drawn
			Ready2:
				FR30 I 1 {
					A_FireCustomMissile("TinyGunSmoker", 0, 0, -4, -6, 0, 0);
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR39");}
					if(GetCvar("pb_toggle_aim_hold")) {
					if(!PressingAltfire() || JustReleased(BT_ALTATTACK)){
						return state("UnequipPistol");
					}
					if (PressingFire() && PressingAltfire() && CountInv("PrimaryPistolAmmo") > 0){
							return state("FirePistol");
					}
					return A_DoPBWeaponAction(WRF_ALLOWRELOAD|WRF_NOSECONDARY, CheckUnloaded("HasUnloadedCryoRifle"));
				}
				else {
					if (PressingFire() && CountInv("PrimaryPistolAmmo") > 0 ){
						return state("FirePistol");
					}
					return A_DoPBWeaponAction(WRF_ALLOWRELOAD, CheckUnloaded("HasUnloadedCryoRifle"));
				}
				return state("");
			}
			Loop
			
			WeaponSpecial:
				TNT1 A 0 A_JumpIfInventory ("GrabbedBarrel", 1, "IdleBarrel")
				TNT1 A 0 A_JumpIfInventory ("GrabbedFlameBarrel", 1, "IdleFlameBarrel")
				TNT1 A 0 A_JumpIfInventory ("GrabbedIceBarrel", 1, "IdleIceBarrel")
				//First, check if player selected different fire mode
				TNT1 A 0 {
					A_StopSound(2);
					A_Takeinventory("ADSmode",1);
					PB_HandleCrosshair(5);
					A_ZoomFactor(1.0);
					A_ClearOverlays(10,11);
				}
				TNT1 A 0
				{
					if (CountInv("CryoRiflePistolToken") >= 1)
					{
						return state("UnequipPistol");
					}
					return state("");
				}
				TNT1 A 0 A_Takeinventory("GoWeaponSpecialAbility",1)
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleMissile_WW",1,"SwitchToMissile")
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleBeam_WW",1,"SwitchToBeam")
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleSpear_WW",1,"SwitchToSpear")
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleFlak_WW",1,"SwitchToFlak")
			SwitchToMissile:
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleMissile",1,"CancelModeSwap")
				TNT1 A 0 {
					A_GiveInventory("FireModeCryoRifleMissile", 1);
					A_TakeInventory("FireModeCryoRifleBeam", 1);
					A_TakeInventory("FireModeCryoRifleSpear", 1);
					A_TakeInventory("FireModeCryoRifleFlak", 1);
					A_Print("Firemode: Ice Missile");
					A_PlaySoundEx("weapons/CryoRifle/up", "Auto");
					A_PlaySoundEx("weapons/CryoRifle/reload1", "Auto");
				}
				FR20 ABCDEDFGHIJKL 1
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect2", "Auto")
				FR20 MNOPQRSTUU 1 A_FireCustomMissile("GunFireSmoke", 0, 0, -5, -5, 0, 0)
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect3", "Auto")
				FR20 VWXYZ 1 A_FireCustomMissile("GunFireSmoke", 0, 0, -5, -5, 0, 0)
				FR21 ABCDEDFGHIJKLMNOPQRS 1
				Goto FinishModeSwap
			SwitchToBeam:
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleBeam",1,"CancelModeSwap")
				TNT1 A 0 {
					A_TakeInventory("FireModeCryoRifleMissile", 1);
					A_GiveInventory("FireModeCryoRifleBeam", 1);
					A_TakeInventory("FireModeCryoRifleSpear", 1);
					A_TakeInventory("FireModeCryoRifleFlak", 1);
					A_Print("Firemode: Ice Beam");
					A_PlaySoundEx("weapons/CryoRifle/up", "Auto");
					A_PlaySoundEx("weapons/CryoRifle/reload1", "Auto");
				}
				FR20 ABCDEDFGHIJKL 1
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect2", "Auto")
				FR20 MNOPQRSTUU 1 A_FireCustomMissile("GunFireSmoke", 0, 0, -5, -5, 0, 0)
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect3", "Auto")
				FR20 VWXYZ 1 A_FireCustomMissile("GunFireSmoke", 0, 0, -5, -5, 0, 0)
				FR21 ABCDEDFGHIJKLMNOPQRS 1
				Goto FinishModeSwap
			SwitchToSpear:
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleSpear",1,"CancelModeSwap")
				TNT1 A 0 {
					A_TakeInventory("FireModeCryoRifleMissile", 1);
					A_TakeInventory("FireModeCryoRifleBeam", 1);
					A_GiveInventory("FireModeCryoRifleSpear", 1);
					A_TakeInventory("FireModeCryoRifleFlak", 1);
					A_Print("Firemode: Ice Spear");
					A_PlaySoundEx("weapons/CryoRifle/up", "Auto");
					A_PlaySoundEx("weapons/CryoRifle/reload1", "Auto");
				}
				FR22 ABCDEFGHIJ 1 
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect2", "Auto")
				FR22 KLMNOPQRSTT 1 A_FireCustomMissile("GunFireSmoke", 0, 0, -5, -5, 0, 0)
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect3", "Auto")
				FR22 UVWXYZ 1 A_FireCustomMissile("GunFireSmoke", 0, 0, -5, -5, 0, 0)
				FR23 ABCDEFGHIJKLMNOPQ 1
				Goto FinishModeSwap
			SwitchToFlak:
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleFlak",1,"CancelModeSwap")
				TNT1 A 0 {
					A_TakeInventory("FireModeCryoRifleMissile", 1);
					A_TakeInventory("FireModeCryoRifleBeam", 1);
					A_TakeInventory("FireModeCryoRifleSpear", 1);
					A_GiveInventory("FireModeCryoRifleFlak", 1);
					A_Print("Firemode: Ice Flak");
					A_PlaySoundEx("weapons/CryoRifle/up", "Auto");
					A_PlaySoundEx("weapons/CryoRifle/reload1", "Auto");
				}
				FR22 ABCDEFGHIJ 1 
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect2", "Auto")
				FR22 KLMNOPQRSTT 1 A_FireCustomMissile("GunFireSmoke", 0, 0, -5, -5, 0, 0)
				TNT1 A 0 A_PlaySoundEx("weapons/CryoRifle/respect3", "Auto")
				FR22 UVWXYZ 1 A_FireCustomMissile("GunFireSmoke", 0, 0, -5, -5, 0, 0)
				FR23 ABCDEFGHIJKLMNOPQ 1
				Goto FinishModeSwap
			FinishModeSwap:
				TNT1 A 0 { 
					A_TakeInventory("FireModeCryoRifleMissile_WW", 1);
					A_TakeInventory("FireModeCryoRifleBeam_WW", 1);
					A_TakeInventory("FireModeCryoRifleSpear_WW", 1);
					A_TakeInventory("FireModeCryoRifleFlak_WW", 1);
				}
				Goto Ready3
			CancelModeSwap:
				TNT1 A 0 { 
					A_Print("Already Selected");
					A_TakeInventory("FireModeCryoRifleMissile_WW", 1);
					A_TakeInventory("FireModeCryoRifleBeam_WW", 1);
					A_TakeInventory("FireModeCryoRifleSpear_WW", 1);
					A_TakeInventory("FireModeCryoRifleFlak_WW", 1);
				}
				Goto Ready3

			Select:
				TNT1 A 0 {
					A_WeaponOffset(0,32);
					A_SetRoll(0);
					PB_HandleCrosshair(79);
					A_TakeInventory("PB_LockScreenTilt",1);
				}
				
				TNT1 A 0 A_TakeInventory("HasBarrel",1)
				TNT1 A 0 A_TakeInventory("HasIceBarrel",1)
				TNT1 A 0 A_TakeInventory("HasFlameBarrel",1)
				TNT1 A 0 A_TakeInventory("GrabbedBarrel",1)
				TNT1 A 0 A_TakeInventory("GrabbedIceBarrel",1)
				TNT1 A 0 A_TakeInventory("GrabbedFlameBarrel",1)
				
				Goto SelectFirstPersonLegs
				SelectContinue:
				TNT1 A 0 PB_WeapTokenSwitch("CryoRifleSelected")
				TNT1 AAAAAAAAAAAAAAAAAA 0 A_Raise
				TNT1 AAAAAAAA 1 A_Raise
				Wait

			Deselect:
				TNT1 A 0 A_JumpIfInventory ("GrabbedBarrel", 1, "PlaceBarrel")
				TNT1 A 0 A_JumpIfInventory ("GrabbedFlameBarrel", 1, "PlaceFlameBarrel")
				TNT1 A 0 A_JumpIfInventory ("GrabbedIceBarrel", 1, "PlaceIceBarrel")
				TNT1 A 0 {
					A_WeaponOffset(0,32);
					A_SetRoll(0);
					PB_HandleCrosshair(79);
					A_TakeInventory("PB_LockScreenTilt",1);
					A_StopSound(2);
				}
				TNT1 A 0 A_Takeinventory("HasUnloadedCryoRifle",1)
				TNT1 A 0 A_Takeinventory("Unloading",1)
				TNT1 A 0 A_StopSound(1)
				TNT1 A 0 A_StopSound(6)
				TNT1 A 0 A_JumpIfInventory("CryoRiflePistolToken",1,1)
				Goto FinalDeselect
				FR30 JKLMNOPQ 1 {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR39");}
				}
			FinalDeselect:
				TNT1 A 0 A_Takeinventory("CryoRiflePistolToken",1)
				FR93 NOPQ 0
				FR03 NOPQ 1 {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR93");}
				}
				TNT1 A 0 A_Takeinventory("CryoRifleOverCooling",420)
				TNT1 AAAAAAAAAAAAAAAAAA 0 A_Lower
				Wait

			Fire:
				TNT1 A 0 A_JumpIfInventory ("GrabbedBarrel", 1, "ThrowBarrel")
				TNT1 A 0 A_JumpIfInventory ("GrabbedFlameBarrel", 1, "ThrowFlameBarrel")
				TNT1 A 0 A_JumpIfInventory ("GrabbedIceBarrel", 1, "ThrowIceBarrel")
				TNT1 A 0 {
					A_WeaponOffset(0,32);
					A_SetRoll(0);
					PB_HandleCrosshair(79);
					A_TakeInventory("PB_LockScreenTilt",1);
				}
				TNT1 A 0 A_JumpIfInventory("CryoRiflePistolToken",1,"FirePistol")
				TNT1 A 0 A_JumpIf(GetCvar("pb_fastcryowarmup") == 1,"FastWarmupCheck")
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleMissile",1,"FireMissile")
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleBeam",1,"FireBeam")
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleSpear",1,"FireSpear")
				TNT1 A 0 A_JumpIfInventory("FireModeCryoRifleFlak",1,"FireFlak")
/////Missile Fire/////
			FireMissile:
				TNT1 A 0 A_JumpIfInventory("CryoRifleAmmo",5,1)
				Goto Reload
				TNT1 A 0 A_JumpIf(GetCvar("pb_fastcryowarmup") == 2,"HoldMissile")
				TNT1 A 0 A_AlertMonsters
				TNT1 A 0 A_Playsound("weapons/CryoRifle/powerup")
				FR10 IIIIIHHGGFFEEDDCCBBAAIIIIIIII 1 {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR11");}} 
			HoldMissile:
				TNT1 AAAAAAAAAA 0 A_FireCustomMissile("BluePlasmaParticle",random(10,-10),0,0,random(-3,3),0,random(7,-7) )
				FR11 ABCDEFGHI 0 // Initialize sprites into virtual memory
				TNT1 A 0 A_AlertMonsters
				FR10 A 1 Bright {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR11");}
					A_GunFlash;
					A_Takeinventory("CryoRifleAmmo",5);
					A_Giveinventory("CryoRifleOverCooling",60);
					A_AlertMonsters(450);
					A_Playsound("weapons/CryoRifle/missile");
					A_FireCustomMissile("IceMissile", 0, 1, 0, 0);
					A_FireCustomMissile("GunFireSmoke", 0, 0, 0, 1, 0, 0);
					A_FireCustomMissile("GunFireSmoke", 0, 0, 0, 1, 0, 0);
					A_FireCustomMissile("GunFireSmoke", 0, 0, 0, 1, 0, 0);
					A_ZoomFactor(0.98);
					if (CountInv("PowerStrength") >= 1 ) {
						A_SetPitch(pitch-0.32, SPF_INTERPOLATE);
						A_SetAngle(angle+0.15, SPF_INTERPOLATE);
					}
					else {
						A_SetPitch(pitch-1.24, SPF_INTERPOLATE);
						A_SetAngle(angle+0.6, SPF_INTERPOLATE);
					}
				}
				FR10 B 1 Bright {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR11");}
					A_ZoomFactor(0.99);
					if (CountInv("PowerStrength") >= 1 ) {
						A_SetPitch(pitch-0.32, SPF_INTERPOLATE);
						A_SetAngle(angle+0.15, SPF_INTERPOLATE);
					}
					else {
						A_SetPitch(pitch-1.24, SPF_INTERPOLATE);
						A_SetAngle(angle+0.6, SPF_INTERPOLATE);
					}
				}
				FR10 C 1 Bright {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR11");}
					A_ZoomFactor(1.0);
				} 
				FR10 D 1 Bright {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR11");}
				}
				FR10 EFGHII 1 {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR11");}
				}
				TNT1 A 0 A_JumpIfInventory("CryoRifleAmmo",5,1)
				Goto EndMissile
				TNT1 A 0
				TNT1 A 0 A_Refire ("HoldMissile")
				Goto EndMissile
			EndMissile:
				TNT1 A 0 A_StopSound(2)
				TNT1 A 0 A_StopSound(6)
				TNT1 A 0 A_Playsound("weapons/CryoRifle/powerdown")
				FR10 CDEFGHI 1 {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR11");}
				}
				TNT1 A 0 A_JumpIfInventory("CryoRifleAmmo",5,1)
				Goto Reload
				TNT1 A 0
				Goto Ready3
/////Beam Fire/////
			FireBeam:
				TNT1 A 0 A_JumpIfInventory("CryoRifleAmmo",1,1)
				Goto Reload
				TNT1 A 0 A_JumpIf(GetCvar("pb_fastcryowarmup") == 2,"HoldBeam")
				TNT1 A 0 A_Playsound("weapons/CryoRifle/powerup")
				TNT1 A 0 A_AlertMonsters
				FR10 IIIIIHHGGFFEEDDCCBBAAIIIIIIII 1 {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR11");}}
			HoldBeam:
				TNT1 A 0 A_Playsound("weapons/cryobowflyby",2,1,1)
				FR14 ABC 0 
				TNT1 A 0 A_AlertMonsters
				FR13 ABCB 1 BRIGHT {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR14");}
					A_GunFlash;
					A_Overlay(-2, "BeamMuzzleFlash");
					A_AlertMonsters(450);
					A_FireCustomMissile("BlueFlareSpawn",0,0,0,0);
					PB_FireCryoRifleBeam();
					A_FireCustomMissile("GunFireSmoke", 0, 0, 0, 1, 0, 0);
					A_Giveinventory("CryoRifleOverCooling",8);
					if (CountInv("PowerStrength") >= 1 ) {
						A_SetPitch(pitch-0.20, SPF_INTERPOLATE);
						A_SetAngle(angle+frandom(-0.3,0.3), SPF_INTERPOLATE);
					}
					else {
						A_SetPitch(pitch-0.40, SPF_INTERPOLATE);
						A_SetAngle(angle+frandom(-0.6,0.6), SPF_INTERPOLATE);
					}
				}
				TNT1 A 0 A_Takeinventory("CryoRifleAmmo",1)
				TNT1 A 0 A_JumpIfInventory("CryoRifleAmmo",1,1)
				Goto EndBeam
				TNT1 A 0
				TNT1 A 0 A_Refire ("HoldBeam")
				Goto EndBeam
			EndBeam:
				TNT1 A 0 A_StopSound(2)
				TNT1 A 0 A_StopSound(6)
				TNT1 A 0 A_Playsound("weapons/CryoRifle/powerdown")
				FR10 CDEFGHI 1 {
					if (CountInv("CryoRifleOverCooling") >= 100 ) { A_SetWeaponSprite("FR11");}
				}
				TNT1 A 0 A_JumpIfInventory("CryoRifleAmmo",2,1)
				Goto Reload
				TNT1 A 0
				Goto Ready3
			BeamMuzzleFlash:
				TNT1 A 0 A_Jump(256, "Muzzle1", "Muzzle2", "Muzzle3", "Muzzle4", "Muzzle5", "Muzzle6", "Muzzle7")
			Muzzle1:
				FR12 A 1 BRIGHT
				Stop
			Muzzle2:
				FR12 B 1 BRIGHT
				Stop
			Muzzle3:
				FR12 C 1 BRIGHT
				Stop
			Muzzle4:
				FR12 D 1 BRIGHT
				Stop
			Muzzle5:
				FR12 E 1 BRIGHT
				Stop
			Muzzle6:
				FR12 F 1 BRIGHT
				Stop
			Muzzle7:
				FR12 G 1 BRIGHT
				Stop
