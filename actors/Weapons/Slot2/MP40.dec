// 			Goto InsertBullets

		ReloadUnloaded:
			TNT1 A 0 A_GiveInventory("MP40WasEmpty", 1)
			MR21 ABC 1
			TNT1 A 0 A_PlaySoundEx("weapons/MP40_up", "Auto")
			MR21 DEFG 1
