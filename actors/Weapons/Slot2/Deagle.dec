		TNT1 A 0 A_ClearOverlays(10,11)
		TNT1 A 0 A_JumpIfInventory("DualWieldingDeagles", 1, "FlashPunchingDW")
		D8E0 ABCDEFGGFEDCBA 1
		Stop
