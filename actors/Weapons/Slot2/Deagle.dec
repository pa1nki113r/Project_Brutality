ACTOR DeagleAmmo : Ammo
{
   Inventory.Amount 0
   Inventory.MaxAmount 8
   Ammo.BackpackAmount 0
   Ammo.BackpackMaxAmount 8
   +INVENTORY.IGNORESKILL
   Inventory.Icon "D4E0Z0"
}

ACTOR LeftDeagleAmmo : Ammo
{
   Inventory.Amount 0
   Inventory.MaxAmount 8
   Ammo.BackpackAmount 0
   Ammo.BackpackMaxAmount 8
   +INVENTORY.IGNORESKILL
   Inventory.Icon "D4E0Z0"
}
