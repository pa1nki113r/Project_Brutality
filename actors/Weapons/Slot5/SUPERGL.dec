	AltFire:
		TNT1 A 0 A_WeaponOffset(0,32)
		TNT1 A 0 A_JumpIfInventory("GrenadeTypeImpact",1, "DetonateImpact")
		TNT1 A 0 A_JumpIfInventory("GrenadeTypeLVG",1, "DetonateLVG")
		TNT1 A 0 A_JumpIfInventory("GrenadeTypeIncendiary",1, "DetonateIncendiary")
		TNT1 A 0 A_JumpIfInventory("GrenadeTypeCryo",1, "DetonateCryo")
		TNT1 A 0 A_JumpIfInventory("GrenadeTypeAcid",1, "DetonateAcid")
		MGLG A 1
		MGLG B 0
		MGLG B 0 Thing_Projectile(1999,201,0,0,0)
		DETO B 0 A_PlayWeaponSound("weapons/pbarm")
		MGLG G 2 Thing_ReMove(1999)
		Goto Ready+7
		
	DetonateImpact:
