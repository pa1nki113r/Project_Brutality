{
Inventory.MaxAmount 1
}

ACTOR GrenadeTypeLVG : Inventory
{
Inventory.MaxAmount 1
}

ACTOR GrenadeTypeIncendiary : Inventory
{
Inventory.MaxAmount 1
}

ACTOR GrenadeTypeCryo : Inventory
{
Inventory.MaxAmount 1
}

ACTOR GrenadeTypeAcid : Inventory
{
Inventory.MaxAmount 1
}

ACTOR GrenadeRounds : Ammo
{
   Inventory.Amount 0
   Inventory.MaxAmount 6
   Ammo.BackpackAmount 0
   Ammo.BackpackMaxAmount 6
}

ACTOR Super_Grenade_Launcher : Weapon 13206
{
	SpawnID 9520
	Weapon.BobRangeX 0.3
	Weapon.BobRangeY 0.5
	Weapon.BobStyle InverseSmooth
	Weapon.BobSpeed 2.4
	Game Doom
	SpawnID 29
	Speed 20
	Damage 20
	Scale .74
	Weapon.SelectionOrder 2500
	Weapon.AmmoUse1 0
	Weapon.AmmoUse2 0
	Weapon.AmmoGive2 0
	Weapon.AmmoGive1 6
	Weapon.AmmoType1 "SpecialGrenadeRounds"
	Weapon.AmmoType2 "GrenadeRounds"
    Inventory.PickupSound "misc/rockboxa"
    +WEAPON.NOAUTOAIM
	+WEAPON.EXPLOSIVE
	+WEAPON.NOALERT
	+FLOORCLIP
	//+DONTGIB
	Inventory.PickupMessage "You got the UAC-MGL automatic grenade launcher! (Slot 5)"
	Tag "Super Grenade Launcher"
	States
	{
	Steady:
	TNT1 A 1
	Goto Ready
	
	GoToPurist:
		TNT1 A 0 A_SelectWeapon("Rocket_Launcher")
