	Fire:
		TNT1 A 0 A_WeaponOffset(0,32)
        TNT1 A 0 A_JumpIfInventory("GrenadeRounds",1,2)
        Goto Reload
		TNT1 A 0 A_JumpIfInventory("GrenadeTypeImpact",1, "FireImpact")
		TNT1 A 0 A_JumpIfInventory("GrenadeTypeLVG",1, "FireLVG")
		TNT1 A 0 A_JumpIfInventory("GrenadeTypeIncendiary",1, "FireIncendiary")
		TNT1 A 0 A_JumpIfInventory("GrenadeTypeCryo",1, "FireCryo")
		TNT1 A 0 A_JumpIfInventory("GrenadeTypeAcid",1, "FireAcid")
        TNT1 AAAA 0
	FireImpact:
		TNT1 AAAA 0
		TNT1 A 0 A_FireCustomMissile("RedFlareSpawn",-5,0,0,0)
        TNT1 A 0 A_PlaySound ("weapons/firegrenade", 18)
		TNT1 A 0 A_AlertMonsters
		TNT1 A 0 BRIGHT A_FireCustomMissile("Alerter")
		MGLF A 1 BRIGHT A_FireCustomMissile("ImpactGrenade", 0, False, 0, 0, 0, 0)
		TNT1 A 0 A_TakeInventory("GrenadeRounds",1)
		TNT1 A 0 A_FireCustomMissile("ShakeYourAssDouble", 0, 0, 0, 0)
		TNT1 A 0 A_FireCustomMissile("GrenadeCaseSpawn",5,0,-10,-5)
