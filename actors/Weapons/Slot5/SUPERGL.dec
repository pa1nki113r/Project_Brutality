        MGLF E 1
        MGLG BCD 2 A_WeaponReady(WRF_NOPRIMARY)
		MGLG EF 1 A_WeaponReady(WRF_NOPRIMARY)
		TNT1 A 0 A_PlaySound("RLCYCLE2", 5)
        MGLG A 1 A_WeaponReady(WRF_NOPRIMARY)
