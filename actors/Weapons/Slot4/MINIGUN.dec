{
	//$Title Minigun Upgrade
	//$Category Project Brutality - Weapon Upgrades
	Game Doom
	SpawnID 9410
	Height 24
	//-COUNTITEM
	+INVENTORY.ALWAYSPICKUP
	+COUNTITEM
	Inventory.Pickupsound "CHGNPKUP"
	Inventory.PickupMessage "You got the Minigun Upgrade!!!"
	Scale 0.52
	FloatBobStrength 0.5
	States
	{
