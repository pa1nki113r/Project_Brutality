Actor PB_HDMRUpgrade: PB_UpgradeItem
{
	//$Title Heavy DMR Upgrade
	//$Category Project Brutality - Weapon Upgrades
	//$Sprite HIFLA0
	Game Doom
	//SpawnID 9410
	Height 24
	//-COUNTITEM
	Scale 0.43
	-INVENTORY.ALWAYSPICKUP
	-COUNTITEM
	Inventory.Pickupsound "weapons/rifle/up"
	Inventory.PickupMessage "Heavy DMR (Slot 4, Upgrade)"
	Tag "Heavy DMR"
	States
	{
		Spawn:
			VHFL A 0 NODELAY
			HIFL A 10 A_PBVPFramework("VHFL")
			"####" "#" 0 A_PbvpInterpolate()
			Loop
		Pickup:
			TNT1 A 0 A_JumpIf(!FindInventory("PB_DMR") || !FindInventory("DMRUpgraded") || CountInv("PB_HighCalMag") < GetAmmoCapacity("PB_HighCalMag"),1)
			fail
			TNT1 A 0
			TNT1 A 0 A_SetInventory("UnderBarrelGrenadeLoaded", 1)
			TNT1 A 0 A_GiveInventory("PB_DMR", 1)
			TNT1 A 0 A_SetInventory("DMRUpgraded", 1)
			Stop
	}
}
