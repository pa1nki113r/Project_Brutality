        TNT1 A 0 A_JumpIfInventory("XRifleAmmo",1,2)
        Goto Reload
        TNT1 AAAA 0
        TNT1 A 0 A_JumpIfInventory("SemiFire",1,"FireSemi")
        TNT1 A 0 A_JumpIfInventory("BurstFire",1,"FireBurst")
		TNT1 A 0 A_JumpIfInventory("Zoomed",1,"Fire2")
