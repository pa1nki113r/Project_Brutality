		TNT1 AAAA 1 A_Jump(255, "Ready")
		Loop
		
	TryToSelectDrum2:
		TNT1 A 0 A_TakeInventory("Kicking",1)
		TNT1 A 0 A_TakeInventory("Salute1", 1)
		TNT1 A 0 A_TakeInventory("Salute2", 1)
