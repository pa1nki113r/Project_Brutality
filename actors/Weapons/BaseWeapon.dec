		TNT1 A 0 A_JumpIfInventory("ProximityMineSelected",1, "SelectStunGrenade")
		TNT1 A 0 A_JumpIfInventory("StunGrenadeSelected",1, "SelectLeech")
		TNT1 A 0 A_JumpIfInventory("LeechSelected", 1, "SelectHealthBackPack")
		TNT1 A 0 A_JumpIfInventory("HealthBackPackSelected", 1, "SelectFragGrenade")
		Goto GoingToReady
		
