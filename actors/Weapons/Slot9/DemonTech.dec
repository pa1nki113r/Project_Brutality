Actor HomingSoul//: HolySpirit
{
  Speed 45
  Height 24
  Radius 24
  Damage 0
  DeathSound ""
  Scale 0.95
  ProjectileKickback 0
  PROJECTILE
  BounceFactor 2.0
  WallBounceFactor 1
  BounceCount 25
  DamageType "Saw"
  //-NOGRAVITY
  +RIPPER
	+NOBOSSRIP
  +NOEXTREMEDEATH
  +BLOODSPLATTER
