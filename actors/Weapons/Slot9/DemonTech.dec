    BLHS B 1 A_JumpIfTracerCloser(PSGH_JITC_DISTANCE, "Possess")
	TNT1 A 0 A_SeekerMissile (4,8)
    TNT1 A 0 A_GiveInventory("PossessionGhostRoutine", 1)
    BLHS C 1 A_JumpIfTracerCloser(PSGH_JITC_DISTANCE, "Possess")
	TNT1 A 0 A_SeekerMissile (4,8)
    TNT1 A 0 A_GiveInventory("PossessionGhostRoutine", 1)
    BLHS C 1 A_JumpIfTracerCloser(PSGH_JITC_DISTANCE, "Possess")
	TNT1 A 0 A_SeekerMissile (4,8)
