ACTOR DyingBruiser1
{
	Game Doom
	Radius 16
	Height 50
	Health 100
	damagefactor "Blood", 0.0 damagefactor "BlueBlood", 0.0 damagefactor "GreenBlood", 0.0
    damagefactor "Avoid", 0.0
	damagefactor "killme", 0.0
	damagefactor "Shrapnel", 0.0
	damagefactor "KillMe", 0.0
	damagefactor "Cutless", 0.6
    BloodType "Green_Blood", "GreenSawBlood", "GreenSawBlood"
	BloodColor "DarkGreen"
	Mass 999999
	-SOLID
	+SHOOTABLE
	+FLOORCLIP
	+NOPAIN
	+DONTSPLASH
	Scale 1.1
	States
	{
	Spawn:
	B5DR AB 6 A_SpawnItem("HeadshotTarget4b", 0, 50,0)
    TNT1 AA 0 A_CustomMissile ("Green_FlyingBlood", 52, 5, random (0, 360), 2, random (0, 160))
    TNT1 AAA 0 A_CustomMissile ("Green_LiquidBlood", 55, 12, random (0, 360), 2, random (0, 160))
	TNT1 A 0 A_PlaySound ("superbaron/pain")
	B5DR CB 6 A_SpawnItem("HeadshotTarget4b", 0, 50,0)
    TNT1 AA 0 A_CustomMissile ("Green_FlyingBlood", 52, 5, random (0, 360), 2, random (0, 160))
    TNT1 AAA 0 A_CustomMissile ("Green_LiquidBlood", 55, 12, random (0, 360), 2, random (0, 160))
	
	B5DR AB 6 A_SpawnItem("HeadshotTarget4b", 0, 50,0)
    TNT1 AA 0 A_CustomMissile ("Green_FlyingBlood", 52, 5, random (0, 360), 2, random (0, 160))
    TNT1 AAA 0 A_CustomMissile ("Green_LiquidBlood", 55, 12, random (0, 360), 2, random (0, 160))
	TNT1 A 0 A_PlaySound ("superbaron/pain")
	B5DR CB 6 A_SpawnItem("HeadshotTarget4b", 0, 50,0)
    TNT1 AA 0 A_CustomMissile ("Green_FlyingBlood", 52, 5, random (0, 360), 2, random (0, 160))
    TNT1 AAA 0 A_CustomMissile ("Green_LiquidBlood", 55, 12, random (0, 360), 2, random (0, 160))
	
	
	B5DR AB 6 A_SpawnItem("HeadshotTarget4b", 0, 50,0)
    TNT1 AA 0 A_CustomMissile ("Green_FlyingBlood", 52, 5, random (0, 360), 2, random (0, 160))
    TNT1 AAA 0 A_CustomMissile ("Green_LiquidBlood", 55, 12, random (0, 360), 2, random (0, 160))
	TNT1 A 0 A_PlaySound ("superbaron/pain")
	B5DR CB 6 A_SpawnItem("HeadshotTarget4b", 0, 50,0)
    TNT1 AA 0 A_CustomMissile ("Green_FlyingBlood", 52, 5, random (0, 360), 2, random (0, 160))
    TNT1 AAA 0 A_CustomMissile ("Green_LiquidBlood", 55, 12, random (0, 360), 2, random (0, 160))
	
	
	B5DR AB 6 A_SpawnItem("HeadshotTarget4b", 0, 50,0)
    TNT1 AA 0 A_CustomMissile ("Green_FlyingBlood", 52, 5, random (0, 360), 2, random (0, 160))
    TNT1 AAA 0 A_CustomMissile ("Green_LiquidBlood", 55, 12, random (0, 360), 2, random (0, 160))
	TNT1 A 0 A_PlaySound ("superbaron/pain")
	B5DR CB 6 A_SpawnItem("HeadshotTarget4b", 0, 50,0)
    TNT1 AA 0 A_CustomMissile ("Green_FlyingBlood", 52, 5, random (0, 360), 2, random (0, 160))
    TNT1 AAA 0 A_CustomMissile ("Green_LiquidBlood", 55, 12, random (0, 360), 2, random (0, 160))
	
	
	B5DR AB 6 A_SpawnItem("HeadshotTarget4b", 0, 50,0)
    TNT1 AA 0 A_CustomMissile ("Green_FlyingBlood", 52, 5, random (0, 360), 2, random (0, 160))
    TNT1 AAA 0 A_CustomMissile ("Green_LiquidBlood", 55, 12, random (0, 360), 2, random (0, 160))
	B5DR CB 6 A_SpawnItem("HeadshotTarget4b", 0, 50,0)
    TNT1 AA 0 A_CustomMissile ("Green_FlyingBlood", 52, 5, random (0, 360), 2, random (0, 160))
    TNT1 AAA 0 A_CustomMissile ("Green_LiquidBlood", 55, 12, random (0, 360), 2, random (0, 160))
	Goto Death
	
	Death:
	TNT1 A 0 A_NoBlocking
	TNT1 A 0 A_Fall
	TNT1 A 0 A_PlaySound("superbaron/death",10)
	B5DR DE 6
	TNT1 A 0 A_PlaySound("BIG_BODYF", 9)
	EXPL A 0 Radius_Quake (2, 12, 0, 8, 0)
	B5DR F 6
	TNT1 A 0 A_SpawnItem("DeadBruiserNoArm")
	Stop
	
	Death.Head:
	TNT1 A 0 A_NoBlocking
	TNT1 A 0 A_Fall
	TNT1 AAAAA 0 A_CustomMissile ("GreenBloodMistBig", 50, 0, random (0, 360), 2, random (10, 170))
	TNT1 A 0 A_SpawnItem("InfernusHeadExplode", 0, 56)
	B5DR GHI 6
	TNT1 A 0 A_SpawnItem ("CrueltyBonus5Health")
	TNT1 A 0 A_SpawnItem("DeadBruiserNoArmNoHead")
	Stop
	
	Death.Decapitate:
	TNT1 A 0 A_NoBlocking
	TNT1 A 0 A_Fall
	TNT1 AAAAA 0 A_CustomMissile ("GreenBloodMistBig", 50, 0, random (0, 360), 2, random (10, 170))
	TNT1 AAAAA 0 A_CustomMissile ("Xdeath1Green", 62, 0, random (0, 360), 2, random (50, 130))
	TNT1 A 0 A_CustomMissile ("XDeathInfernusHead", 62, 0, random (0, 360), 2, random (50, 130))
	BTN4 ABCD 6
	TNT1 A 0 A_SpawnItem ("CrueltyBonus5Health")
	TNT1 A 0 A_SpawnItem("DeadBruiserNoArmNoHead")
	Stop
	
	
	XDeath:
	Death.ExplosiveImpact:
		TNT1 A 0 A_SpawnItem ("CrueltyBonus5Health")
		TNT1 A 0 A_NoBlocking
	    TNT1 A 0 A_CustomMissile ("InfernusXDeath", 0, 0, random (0, 360), 2, random (0, 160))
	    Stop
	
  Death.ExplosiveImpact:
  Death.ExtremePunches:
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 ThrustThingZ(0,35,0,1)
		TNT1 AAA 0 A_CustomMissile ("XDeath3Green", 62, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("XDeathInfernusLeg", 32, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAA 0 A_CustomMissile ("XDeath1Green", 62, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAA 0 A_CustomMissile ("MuchBloodGreen", 62, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_Jump(255, "Death.ExplosiveImpact1", "Death.ExplosiveImpact2")
    Death.ExplosiveImpact1:	
		B103 B  8 A_NoBlocking
		B103 CCCCCC  8 A_CheckFloor("Land1")
		Goto Land1
	
	Land1:
		TNT1 A 0 A_SpawnItem ("DeadBruiserExplosiveImpact1", 1)
	    Stop
	
	Death.ExplosiveImpact2:	
		B103 F  8 A_NoBlocking
		B103 GGGGGG  8 A_CheckFloor("Land2")
		Goto Land2
	
	Land2:
		TNT1 A 0 A_SpawnItem ("DeadBruiserExplosiveImpact2", 1)
	    Stop
		
	Death.SSG:
	Death.Railgun:
	    TNT1 A 0
		TNT1 AAAA 0 A_CustomMissile ("SuperWallGreenBlood", 32, 0, random (170, 190), 2, random (0, 40))
		TNT1 AAAA 0 A_CustomMissile ("GreenBloodMistBig", 40, 0, random (0, 360), 2, random (30, 90))
		TNT1 A 0 A_CustomMissile ("XDeathStomach", 60, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAA 0 A_CustomMissile ("XDeath3Green", 45, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAA 0 A_CustomMissile ("XDeath2Green", 45, 0, random (0, 360), 2, random (0, 160))
        TNT1 A 0 A_NoBlocking
	    TNT1 A 0 A_BossDeath
		TNT1 AAAAAA 0 A_CustomMissile ("Xdeath1Green", 40, 0, random (0, 360), 2, random (0, 160))
        TNT1 AAAAA 0 A_CustomMissile ("Green_FlyingBlood", 15, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAAA 0 A_CustomMissile ("Green_Instestin", 32, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("XDeathInfernusTorso", 48, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("XDeathInfernusHead", 62, 0, random (0, 360), 2, random (50, 130))
        BT01 AABCD 10
        TNT1 A 0 A_SpawnItem ("DeadBruiserHalfDown", 1)
		TNT1 A 0 A_SpawnItem ("GreenGrowingBloodPool")
		Stop
		
}}

