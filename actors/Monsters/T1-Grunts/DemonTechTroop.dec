Actor Trooperbullet: FastProjectile
{
	Projectile
	+RANDOMIZE
	+FORCEXYBILLBOARD
   +BLOODSPLATTER 
   +NOEXTREMEDEATH
   -TELESTOMP
  +NOTELEFRAG
damage (random(8,11))
damagetype fire
