			if(CountInv("HasPlasmaWeapon",AAPTR_TARGET) == 1)
			{return state("Death.Plasma");}
			if(CountInv("HasIncendiaryWeapon",AAPTR_TARGET) == 1)
			{return state("Death.Incinerate");}
			if(CountInv("RailgunSelected",AAPTR_TARGET) == 1 && CountInv("RailgunLaserMode",AAPTR_TARGET) == 1)
			{return state("Death.Incinerate");}
			if(CountInv("FlameCannonSelected",AAPTR_TARGET) == 1 && CountInv("FlamerNukageMode", AAPTR_TARGET) != 1)
			{return state("Death.Fire");}
			if(CountInv("BHCSelected",AAPTR_TARGET) == 1)
			{return state("Death.Blackhole");}
			if(CountInv("FreezerSelected", AAPTR_TARGET)  == 1 && CountInv("FireModeFreezerBeam", AAPTR_TARGET) == 1 && CountInv("FreezerPistolToken", AAPTR_TARGET) != 1)
			{return state("Death.IceBeam");}
			if(CountInv("FreezerSelected", AAPTR_TARGET)  == 1 && CountInv("FreezerPistolToken", AAPTR_TARGET) != 1)
			{return state("Death.Ice");}
			return state("");
		}
