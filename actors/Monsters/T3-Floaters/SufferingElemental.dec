	  TNT1 AAA 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128) TNT1 A 0 A_CustomMissile ("GreenExplosionFire", 12, 0, random (0, 360), 2, random (0, 160))
