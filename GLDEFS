//Motion Blur by Pixel Eater
HardwareShader PostProcess beforebloom
{
	Name "MBlur"
	Shader "MBlur.fp" 330
	Uniform vec2 steps
	Uniform int samples
	Uniform float increment
	Uniform int blendmode
}

